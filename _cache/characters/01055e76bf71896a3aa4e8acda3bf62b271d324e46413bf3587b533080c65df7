g'\\n             };\\n             if (field === '性别') { // Specific icon based on value for '性别'\\n                 if (value === '男') return 'fa-mars';\\n                 if (value === '女') return 'fa-venus';\\n             }\\n             return iconMap[field] || 'fa-question-circle'; // Default icon\\n        }\\n        // Includes child type icon\\n        function getIconForType(type) {\\n            const iconMap = { 'npc': 'fa-female', 'child': 'fa-child', 'item': 'fa-boxes-stacked' };\\n            return iconMap[type] || 'fa-question-circle';\\n        }\\n        // Generic multi-value formatter\\n        function formatMultiValue(value, field) {\\n             const tagsFields = ['性格', '性癖', '性伴侣', '效果', '态度']; // Added '态度' here just in case it can be multi-value\\n             const valueStr = (value === null || value === undefined) ? '' : String(value);\\n             if (tagsFields.includes(field) && valueStr && valueStr.toLowerCase() !== '无' && valueStr.toLowerCase() !== '未知' && valueStr.includes(',')) { // Only format if likely multi-value\\n                 return valueStr.split(/[,、\\\\s;]+/).map(tag => tag.trim()).filter(tag => tag).map(tag => {\\n                     let tagClass = 'value-tag';\\n                     // Add specific styling for tags if needed (Example for 好感度/态度)\\n                     if (field === '态度' || field === '性格') {\\n                         if (['亲近', '开朗', '活泼', '细心', '独立'].includes(tag)) tagClass += ' positive';\\n                         if (['疏远', '冷漠', '强势', '安静'].includes(tag)) tagClass += ' negative';\\n                     } else if (field === '好感度') {\\n                         const numericValue = parseInt(tag);\\n                         if (!isNaN(numericValue)) {\\n                             if (numericValue > 50) tagClass += ' positive';\\n                             else if (numericValue < 0) tagClass += ' negative';\\n                         }\\n                     }\\n                     return `<span class=\\\"${tagClass}\\\">${tag}</span>`;\\n                 }).join('');\\n             }\\n             // Special formatting for numeric fields maybe? Example:\\n             if (field === '好感度' || field === '服从度' || field === '性欲值' || field === '美貌' || field === '年龄' || field === '数量') {\\n                 // Could add progress bars or specific styling here later\\n             }\\n             return valueStr; // Return plain string otherwise\\n         }\\n\\n\\n        // --- Data Parsing ---\\n        // Parses all known list types, including CHILD\\n        function parseDataString(text) {\\n             console.log(\\\"[DEBUG] Starting parseDataString...\\\");\\n             // ** CONFIRMED: Initializes 'children' array **\\n             const data = { npcs: [], children: [], items: [] };\\n             if (!text || !text.trim()) { console.warn(\\\"[DEBUG] parseDataString: Input text is empty.\\\"); return data; }\\n\\n             // Helper to find the last block of a given type\\n             const findLastBlock = (type, fullText) => {\\n                 const startTag = `>>${type}_LIST_START`; const endTag = `>>${type}_LIST_END`;\\n                 const regex = new RegExp( startTag.replace(/[-\\\\/\\\\\\\\^$*+?.()|[\\\\]{}]/g, '\\\\\\\\$&')+'([\\\\\\\\s\\\\\\\\S]*?)'+endTag.replace(/[-\\\\/\\\\\\\\^$*+?.()|[\\\\]{}]/g, '\\\\\\\\$&'), 'g');\\n                 let lastMatch = null; let match;\\n                 while ((match = regex.exec(fullText)) !== null) { lastMatch = match; }\\n                 if(lastMatch) console.log(`[DEBUG] Found block content for type: ${type}`);\\n                 else console.log(`[DEBUG] No block content found for type: ${type}`);\\n                 return lastMatch ? lastMatch[1] : null;\\n             };\\n\\n             // ** CONFIRMED: Includes 'CHILD' definition **\\n             const sections = {\\n                 'NPC': { key: 'npcs', prefix: 'NPC:' },\\n                 'CHILD': { key: 'children', prefix: 'CHILD:' },\\n                 'ITEM': { key: 'items', prefix: 'ITEM:' }\\n             };\\n\\n             // ** CONFIRMED: Loop iterates over 'CHILD' **\\n             for (const type in sections) {\\n                 console.log(`[DEBUG] Parsing section: ${type}`);\\n                 const content = findLastBlock(type, text);\\n                 if (content) {\\n                     const lines = content.trim().split('\\\\n').map(l => l.trim()).filter(l => l);\\n                     if (lines.length < 1) { console.warn(`[DEBUG] No content lines found for ${type} after trimming.`); continue; }\\n\\n                     const fieldsLine = lines.find(l => l.startsWith('FIELDS:'));\\n                     if (!fieldsLine) { console.warn(`[DEBUG] No FIELDS line for ${type}. Skipping section.`); continue; }\\n                     const fields = fieldsLine.substring(7).split('|').map(f => f.trim()).filter(f => f);\\n                     if (fields.length === 0) { console.warn(`[DEBUG] No valid fields found for ${type}. Skipping section.`); continue;}\\n                     console.log(`[DEBUG] Parsed fields for ${type}: [${fields.join(', ')}]`);\\n\\n                     const dataLines = lines.filter(l => l.startsWith(sections[type].prefix));\\n                     console.log(`[DEBUG] Found ${dataLines.length} data lines starting with '${sections[type].prefix}' for ${type}.`);\\n\\n                     dataLines.forEach((line, lineIndex) => {\\n                         // Split, ensuring we don't create more parts than fields defined\\n                         const values = line.substring(sections[type].prefix.length).split('|', fields.length).map(v => v.trim());\\n                         // Pad if data line has fewer separators than expected fields\\n                         while (values.length < fields.length) { values.push(''); }\\n\\n                         if (values.length === fields.length) {\\n                             const entry = {};\\n                             fields.forEach((field, index) => {\\n                                 entry[field] = values[index]; // Assign even if empty string\\n                             });\\n                             // ** CONFIRMED: Pushes to correct array (data.children for type CHILD) **\\n                             data[sections[type].key].push(entry);\\n                             console.log(`[DEBUG] Successfully parsed ${type} entry ${lineIndex + 1}:`, JSON.stringify(entry));\\n                         } else {\\n                             // This should ideally not happen due to the padding logic above\\n                             console.warn(`[DEBUG] Field/Value length mismatch for ${type} line ${lineIndex + 1} (Fields: ${fields.length}, Values: ${values.length}): ${line}`);\\n                         }\\n                     });\\n                 }\\n             }\\n             console.log(\\\"[DEBUG] Finished parseDataString. Final parsedData:\\\", JSON.parse(JSON.stringify(data))); // Deep copy for logging\\n             return data;\\n        }\\n\\n        // --- Rendering Functions ---\\n        function renderSidebar(data) {\\n             console.log(\\\"[DEBUG] Rendering Sidebar...\\\");\\n             // Log input data structure\\n             console.log(`[DEBUG] Data received by renderSidebar: NPCs=${data?.npcs?.length || 0}, Children=${data?.children?.length || 0}, Items=${data?.items?.length || 0}`);\\n             sidebarContentEl.innerHTML = ''; errorMessageEl.style.display = 'none';\\n\\n             // ** CONFIRMED: Includes 'child' category definition referencing data.children **\\n             const categories = [\\n                 { type: 'npc', title: '女性角色', icon: 'fa-female', data: data.npcs, nameField: '姓名' },\\n                 { type: 'child', title: '子女信息', icon: 'fa-child', data: data.children, nameField: '姓名' },\\n                 { type: 'item', title: '重要物品', icon: 'fa-boxes-stacked', data: data.items, nameField: '物品名' }\\n             ];\\n\\n             let hasData = false;\\n             categories.forEach(cat => {\\n                 console.log(`[DEBUG] Processing category: ${cat.title}`);\\n                 // Explicitly check the data source for the category\\n                 const categoryData = cat.data;\\n                 const isDataValid = Array.isArray(categoryData) && categoryData.length > 0;\\n                 console.log(`[DEBUG] Category ${cat.title}: IsArray=${Array.isArray(categoryData)}, Length=${categoryData?.length || 0}, IsValid=${isDataValid}`);\\n\\n                 // ** CONFIRMED: Correctly checks if data.children is a non-empty array **\\n                 if (isDataValid) {\\n                     hasData = true;\\n                     console.log(`[DEBUG] Rendering category: ${cat.title}`);\\n                     const categoryDiv = document.createElement('div'); categoryDiv.className = 'sidebar-category';\\n                     const titleEl = document.createElement('h3'); titleEl.className = 'category-title'; titleEl.innerHTML = `<i class=\\\"fas ${cat.icon}\\\"></i> ${cat.title}`; categoryDiv.appendChild(titleEl);\\n                     const listEl = document.createElement('ul'); listEl.className = 'category-list';\\n                     categoryData.forEach((item) => {\\n                         const itemName = item[cat.nameField] || `未命名 (${cat.type})`; const itemId = itemName; // Using name as unique ID for simplicity here\\n                         const listItem = document.createElement('li'); listItem.className = 'list-item'; listItem.textContent = itemName; listItem.title = itemName; listItem.dataset.type = cat.type; listItem.dataset.id = itemId;\\n                         listItem.addEventListener('click', () => { displayDetails(cat.type, itemId); }); listEl.appendChild(listItem);\\n                     }); categoryDiv.appendChild(listEl); sidebarContentEl.appendChild(categoryDiv);\\n                 } else {\\n                     console.log(`[DEBUG] Skipping empty or invalid category: ${cat.title}`);\\n                 }\\n             });\\n\\n             if (!hasData) {\\n                 sidebarContentEl.innerHTML = '<div class=\\\"no-data-message\\\">无数据条目</div>';\\n                 console.log(\\\"[DEBUG] No data items rendered in sidebar.\\\");\\n                 clearDetails();\\n             } else {\\n                  // Restore selection if applicable\\n                  if (currentDataType && currentDataId) {\\n                     const selectedElement = sidebarContentEl.querySelector(`.list-item[data-type=\\\"${currentDataType}\\\"][data-id=\\\"${CSS.escape(currentDataId)}\\\"]`);\\n                     if (selectedElement) { selectedElement.classList.add('selected'); }\\n                     else { clearDetails(); } // Clear details if selected item disappeared\\n                 } else {\\n                      clearDetails(); // Ensure details are clear if no selection state exists\\n                 }\\n             }\\n             console.log(\\\"[DEBUG] Sidebar rendering complete.\\\");\\n        }\\n\\n        function displayDetails(type, id) {\\n             console.log(`[DEBUG] Displaying details for type: ${type}, id: ${id}`);\\n             let dataSet; let nameField;\\n\\n             // ** CONFIRMED: Handles 'child' type correctly **\\n             if (type === 'npc') { dataSet = parsedData.npcs; nameField = '姓名'; }\\n             else if (type === 'child') { dataSet = parsedData.children; nameField = '姓名'; }\\n             else if (type === 'item') { dataSet = parsedData.items; nameField = '物品名'; }\\n             else { console.error(\\\"Unknown data type:\\\", type); clearDetails(); resetSelectionState(); return; }\\n\\n             // Ensure dataset is valid before proceeding\\n             if (!Array.isArray(dataSet)) { console.error(`Data set for type '${type}' is not an array.`); clearDetails(); resetSelectionState(); return; }\\n\\n             selectedItem = dataSet.find(item => item[nameField] === id);\\n\\n             if (!selectedItem) { console.error(`Item not found for type: ${type}, id: ${id}`); clearDetails(); resetSelectionState(); return; }\\n\\n             currentDataType = type; currentDataId = id;\\n\\n             // --- Update Header ---\\n             detailIconEl.className = `fas ${getIconForType(type)} detail-icon`;\\n             detailNameEl.textContent = selectedItem[nameField];\\n             let subtitle = '';\\n             // ** CONFIRMED: Generates subtitle for 'child' correctly **\\n             if (type === 'npc') { subtitle = `${selectedItem['年龄'] || '?'}岁, ${selectedItem['职业'] || '未知职业'}`; }\\n             else if (type === 'child') { subtitle = `${selectedItem['性别'] || '?'}孩, ${selectedItem['年龄'] || '?'}岁${selectedItem['母亲'] ? ', 母亲: '+selectedItem['母亲'] : ''}`; }\\n             else if (type === 'item') { subtitle = `${selectedItem['类别'] || '未知类别'}${selectedItem['稀有度'] ? ', '+selectedItem['稀有度'] : ''}`; }\\n             detailSubtitleEl.textContent = subtitle;\\n\\n             // --- Update Attributes ---\\n             detailAttributesEl.innerHTML = '';\\n             const allFields = Object.keys(selectedItem);\\n             // Define order or filter fields if needed, otherwise display all except nameField\\n             const fieldsToDisplay = allFields.filter(key => key !== nameField); // Exclude the name field itself\\n\\n             fieldsToDisplay.forEach(key => {\\n                 // Only display if the key actually exists on the item (robustness)\\n                 if (!Object.prototype.hasOwnProperty.call(selectedItem, key)) return;\\n\\n                 const value = selectedItem[key];\\n                 const itemDiv = document.createElement('div'); itemDiv.className = 'attribute-item';\\n\\n                 const keyDiv = document.createElement('div'); keyDiv.className = 'attribute-key';\\n                 // ** CONFIRMED: Uses getIconForField which handles child fields **\\n                 keyDiv.innerHTML = `<i class=\\\"fas ${getIconForField(key, value)}\\\"></i> ${key}`;\\n\\n                 const valueDiv = document.createElement('div'); valueDiv.className = 'attribute-value'; valueDiv.dataset.field = key;\\n                 // ** CONFIRMED: Uses formatMultiValue which handles potential child multi-value fields **\\n                 valueDiv.innerHTML = formatMultiValue(value, key);\\n\\n                 itemDiv.appendChild(keyDiv); itemDiv.appendChild(valueDiv); detailAttributesEl.appendChild(itemDiv);\\n             });\\n\\n             // --- Show Details / Hide Placeholder ---\\n             placeholderMessageEl.style.display = 'none'; detailsViewEl.style.display = 'block'; errorMessageEl.style.display = 'none';\\n\\n             // --- Update Sidebar Selection Style ---\\n             document.querySelectorAll('.sidebar .list-item').forEach(el => {\\n                 // Use CSS escape for potentially complex IDs, though names are used here\\n                 const isSelected = el.dataset.type === currentDataType && el.dataset.id === currentDataId;\\n                 el.classList.toggle('selected', isSelected);\\n             });\\n             console.log(\\\"[DEBUG] Details display updated.\\\");\\n        }\\n\\n        // Clears the details view and selection state\\n        function clearDetails() {\\n            console.log(\\\"[DEBUG] Clearing details view.\\\");\\n            placeholderMessageEl.style.display = 'flex'; // Show placeholder\\n            detailsViewEl.style.display = 'none';      // Hide details\\n            errorMessageEl.style.display = 'none';    // Hide error message\\n            resetSelectionState();\\n            // Remove 'selected' class from any previously selected sidebar item\\n            document.querySelectorAll('.sidebar .list-item.selected').forEach(el => {\\n                el.classList.remove('selected');\\n            });\\n        }\\n        // Resets the global selection variables\\n        function resetSelectionState() {\\n            console.log(\\\"[DEBUG] Resetting selection state.\\\");\\n            selectedItem = null;\\n            currentDataType = null;\\n            currentDataId = null;\\n        }\\n\\n\\n        // --- Initialization and Data Handling ---\\n        function processRawData() {\\n            console.log(\\\"--- [DEBUG] Starting processRawData ---\\\");\\n            let rawText = '';\\n            if (dataInjectionEl) {\\n                rawText = dataInjectionEl.textContent || dataInjectionEl.innerText || '';\\n                // Log first 500 chars for inspection without overwhelming console\\n                console.log(`[DEBUG] Read ${rawText.length} chars from #st-data-injection. Start:\\\\n\\\"${rawText.substring(0, 500)}${rawText.length > 500 ? '...' : ''}\\\"`);\\n            } else {\\n                 console.error(\\\"[DEBUG] CRITICAL: Cannot find #st-data-injection!\\\");\\n                 sidebarContentEl.innerHTML = '<div class=\\\"error-message\\\">内部错误: 缺少数据容器</div>';\\n                 errorMessageEl.textContent = '页面结构错误，无法加载数据。';\\n                 errorMessageEl.style.display = 'block';\\n                 clearDetails();\\n                 return;\\n            }\\n\\n            if (!rawText.trim()) {\\n                 console.warn(\\\"[DEBUG] Injected data is empty or whitespace only.\\\");\\n                 sidebarContentEl.innerHTML = '<div class=\\\"no-data-message\\\">未接收到有效数据</div>';\\n                 errorMessageEl.textContent = 'AI回复中未找到匹配的数据块或数据块为空。';\\n                 errorMessageEl.style.display = 'block';\\n                 clearDetails();\\n                 return;\\n            }\\n\\n            try {\\n                // *** Execute Parsing (should handle children correctly) ***\\n                parsedData = parseDataString(rawText);\\n\\n                // *** Execute Rendering (should render children if parsed) ***\\n                renderSidebar(parsedData);\\n\\n                // Attempt to re-select the previously selected item if it still exists\\n                if(currentDataType && currentDataId) {\\n                     console.log(`[DEBUG] Attempting to restore selection post-processing: ${currentDataType} - ${currentDataId}`);\\n                     // Check if the item still exists in the newly parsed data\\n                     let dataSet; let nameField;\\n                     if (currentDataType === 'npc') { dataSet = parsedData.npcs; nameField = '姓名'; }\\n                     else if (currentDataType === 'child') { dataSet = parsedData.children; nameField = '姓名'; }\\n                     else if (currentDataType === 'item') { dataSet = parsedData.items; nameField = '物品名'; }\\n\\n                     const itemStillExists = Array.isArray(dataSet) && dataSet.some(item => item[nameField] === currentDataId);\\n\\n                     if (itemStillExists) {\\n                         // Check if it's already selected by renderSidebar, if not, call displayDetails\\n                         const selectedElement = sidebarContentEl.querySelector(`.list-item[data-type=\\\"${currentDataType}\\\"][data-id=\\\"${CSS.escape(currentDataId)}\\\"].selected`);\\n                         if (selectedElement && detailsViewEl.style.display !== 'none') {\\n                              console.log(\\\"[DEBUG] Selection restored and details view already visible.\\\");\\n                         } else if (selectedElement) {\\n                              console.log(\\\"[DEBUG] Selection restored, ensuring details view is visible.\\\");\\n                              displayDetails(currentDataType, currentDataId); // Re-trigger display if sidebar selected but details hidden\\n                         } else {\\n                              console.log(\\\"[DEBUG] Item exists but was not selected in sidebar. Clearing details.\\\");\\n                              clearDetails(); // Item exists but wasn't selected, might be edge case, clear state.\\n                         }\\n                     } else {\\n                         console.log(\\\"[DEBUG] Previously selected item no longer exists. Clearing details.\\\");\\n                         clearDetails(); // The item is gone, clear the selection and details\\n                     }\\n                } else {\\n                     // No previous selection, state should be clear already from renderSidebar/clearDetails\\n                     console.log(\\\"[DEBUG] No previous selection state to restore.\\\");\\n                     // Ensure details are hidden if no data was rendered or no item selected\\n                     if(detailsViewEl.style.display !== 'none' && !document.querySelector('.sidebar .list-item.selected')) {\\n                        clearDetails();\\n                     }\\n                }\\n\\n            } catch (error) {\\n                 console.error(\\\"[DEBUG] Error processing or rendering data:\\\", error);\\n                 sidebarContentEl.innerHTML = `<div class=\\\"error-message\\\">处理数据时出错: ${error.message}</div>`;\\n                 errorMessageEl.textContent = `处理或渲染数据时发生JavaScript错误: ${error.message}`;\\n                 errorMessageEl.style.display = 'block';\\n                 clearDetails();\\n            }\\n            console.log(\\\"--- [DEBUG] Finished processRawData ---\\\");\\n        }\\n\\n        // --- Initialization ---\\n        function init() {\\n            console.log(\\\"[DEBUG] Initializing Dashboard...\\\");\\n            if (!document.getElementById('st-data-injection')) {\\n                console.error(\\\"CRITICAL: #st-data-injection missing from HTML!\\\");\\n                document.body.innerHTML = '<div class=\\\"error-message\\\" style=\\\"padding: 20px; color: red; font-weight: bold;\\\">错误：HTML结构不完整，无法启动面板。</div>';\\n                return;\\n            }\\n            // Initial processing of potentially pre-loaded data\\n            processRawData();\\n\\n            // Optional: Setup MutationObserver for SillyTavern Auto-Refresh\\n            // This observes changes in the #st-data-injection div's content\\n            if (typeof MutationObserver !== 'undefined') {\\n                const observer = new MutationObserver((mutationsList) => {\\n                    for(let mutation of mutationsList) {\\n                        if (mutation.type === 'childList' || mutation.type === 'characterData') {\\n                            console.log('[DEBUG] Mutation detected on #st-data-injection. Reprocessing...');\\n                            // Use a small delay to allow ST to finish potentially multiple writes\\n                            setTimeout(processRawData, 100);\\n                            break; // Process only once per batch of mutations\\n                        }\\n                    }\\n                });\\n\\n                if (dataInjectionEl) {\\n                    observer.observe(dataInjectionEl, {\\n                        characterData: true, // Observe changes to text content\\n                        childList: true,     // Observe addition/removal of child nodes (less likely but safe)\\n                        subtree: true        // Observe text changes within children too\\n                    });\\n                    console.log(\\\"[DEBUG] MutationObserver attached to #st-data-injection.\\\");\\n                } else {\\n                    console.warn(\\\"[DEBUG] Could not attach MutationObserver: #st-data-injection not found at observer setup time.\\\");\\n                }\\n            } else {\\n                console.warn(\\\"[DEBUG] MutationObserver not supported. Automatic refresh on data change might not work.\\\");\\n            }\\n        }\\n\\n        // --- Run Initialization ---\\n        // Ensure the DOM is fully loaded before running the script\\n        if (document.readyState === 'loading') {\\n            document.addEventListener('DOMContentLoaded', init);\\n        } else {\\n            // DOM is already loaded\\n            init();\\n        }\\n\\n    </script>\\n</body>\\n</html>\\n```\",\"trimStrings\":[],\"placement\":[2],\"disabled\":false,\"markdownOnly\":true,\"promptOnly\":false,\"runOnEdit\":true,\"substituteRegex\":0,\"minDepth\":null,\"maxDepth\":null},{\"id\":\"864aa8f1-c08f-4b26-85c5-97eae9ba3044\",\"scriptName\":\"[低饱和莫兰迪风格]导航模块\",\"findRegex\":\"<map_data>([\\\\s\\\\S]*?)<\\\\/map_data>\",\"replaceString\":\"\\n```html\\n<!DOCTYPE html>\\n<html lang=\\\"zh-CN\\\">\\n<head>\\n    <meta charset=\\\"UTF-8\\\">\\n    <meta name=\\\"viewport\\\" content=\\\"width=device-width, initial-scale=1.0\\\">\\n    <title>恋爱冒险地图</title>\\n    <link rel=\\\"stylesheet\\\" href=\\\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css\\\">\\n    <link href=\\\"https://fonts.googleapis.com/css2?family=ZCOOL+XiaoWei&family=ZCOOL+QingKe+HuangYou&display=swap\\\" rel=\\\"stylesheet\\\">\\n</head>\\n<body>\\n    <div id=\\\"map-data-source\\\" style=\\\"display:none;\\\">$1</div>\\n    <div class=\\\"map-interface\\\">\\n        <div class=\\\"map-header\\\">\\n            <h2 id=\\\"map-title\\\">通用地图导航</h2>\\n        </div>\\n\\n        <!-- 移动限制提示 (这个div仍然存在，但JS逻辑会确保它一直隐藏) -->\\n        <div id=\\\"movement-alert\\\" class=\\\"movement-alert hidden\\\">\\n            <i class=\\\"fas fa-exclamation-triangle\\\"></i> 提示：你现在无法移动        </div>\\n\\n        <!-- 主地点选择区域 -->\\n        <div id=\\\"main-locations\\\" class=\\\"location-grid\\\">\\n            <!-- 主要地点按钮将在这里动态生成 -->\\n        </div>\\n\\n        <!-- 次级地点选择区域 -->\\n        <div id=\\\"sub-locations\\\" class=\\\"sub-location-grid hidden\\\">\\n            <!-- 次级地点按钮将在这里动态生成 -->\\n        </div>\\n\\n        <!-- 地点详情区域 -->\\n        <div id=\\\"location-details\\\" class=\\\"location-details hidden\\\">\\n            <div class=\\\"detail-header\\\">\\n                <i class=\\\"fas fa-map-marker-alt\\\"></i> <span id=\\\"selected-location\\\">未选择</span>\\n            </div>\\n            <div class=\\\"detail-content\\\">\\n                <div class=\\\"character-section\\\">\\n                    <div class=\\\"section-title\\\"><i class=\\\"fas fa-user-friends\\\"></i> 可能遇见的人</div>\\n                    <div id=\\\"location-characters\\\" class=\\\"character-list\\\">\\n                        <!-- 角色列表将在这里动态生成 -->\\n                    </div>\\n                </div>\\n            </div>\\n            <div class=\\\"action-section\\\">\\n                <button id=\\\"confirm-move-btn\\\" class=\\\"action-button\\\">\\n                    <i class=\\\"fas fa-walking\\\"></i> 前往此处\\n                </button>\\n            </div>\\n        </div>\\n    </div>\\n\\n    <!-- 确认移动弹窗 -->\\n    <div id=\\\"confirm-overlay\\\" class=\\\"modal-overlay hidden\\\">\\n        <div class=\\\"confirm-dialog\\\">\\n            <h4>确认前往</h4>\\n            <p id=\\\"confirm-message\\\"></p>\\n            <div class=\\\"dialog-buttons\\\">\\n                <button id=\\\"cancel-move-btn\\\" class=\\\"dialog-button cancel-button\\\">\\n                    <i class=\\\"fas fa-times\\\"></i> 再想想\\n                </button>\\n                <button id=\\\"execute-move-btn\\\" class=\\\"dialog-button confirm-button\\\">\\n                    <i class=\\\"fas fa-check\\\"></i> 出发吧\\n                </button>\\n            </div>\\n        </div>\\n    </div>\\n\\n    <script>\\n        // 全局变量\\n        let mapData = null;\\n        let selectedMain = null;\\n        let selectedSub = null;\\n        let selectedCharacters = [];\\n        let hasMoveBlock = false; // 这个变量依然存在，但会被强制设为false\\n\\n        // 解析地图数据\\n        function parseMapData(text) {\\n            if (!text || typeof text !== 'string') {\\n                console.error(\\\"无效的地图数据\\\", text);\\n                return null;\\n            }\\n\\n            console.log(\\\"开始解析地图数据:\\\", text.substring(0, 50) + \\\"...\\\");\\n\\n            const data = {\\n                moveBlocked: false, // <--- 初始化为 false\\n                locations: []\\n            };\\n\\n            try {\\n                // 解析移动限制状态 - START OF CHANGE\\n                // 下面的代码块被注释掉了，这样 data.moveBlocked 会一直保持 false\\n                /*\\n                const moveBlockMatch = /\\\\[MOVEBLOCK:(YES|NO)\\\\]/.exec(text);\\n                if (moveBlockMatch) {\\n                    // 不再根据匹配结果设置 data.moveBlocked\\n                    // data.moveBlocked = moveBlockMatch[1] === 'YES';\\n                    console.log(\\\"检测到 MOVEBLOCK 标记，但已忽略其状态。\\\");\\n                }\\n                */\\n                // END OF CHANGE - data.moveBlocked 将始终为 false\\n\\n                // 解析地点数据\\n                const locationMatches = Array.from(text.matchAll(/\\\\[((?:[^\\\\[\\\\]]|\\\\[[^\\\\[\\\\]]*\\\\])*?)\\\\]/gs));\\n                console.log(`找到 ${locationMatches.length} 个可能的地点块匹配项`);\\n\\n                for (const match of locationMatches) {\\n                    const locationText = match[1].trim();\\n\\n                    if (locationText.startsWith('MOVEBLOCK:')) {\\n                        console.log(\\\"跳过MOVEBLOCK块\\\");\\n                        continue;\\n                    }\\n\\n                    const lines = locationText.split('\\\\n').map(line => line.trim()).filter(line => line);\\n                    if (lines.length < 1) continue;\\n\\n                    const mainLocation = lines[0];\\n                    if (mainLocation.startsWith('MOVEBLOCK:')) {\\n                        console.log(\\\"在地点块内再次跳过MOVEBLOCK行\\\");\\n                        continue;\\n                    }\\n                    console.log(\\\"处理主地点:\\\", mainLocation);\\n                    const subLocations = [];\\n\\n                    for (let i = 1; i < lines.length; i++) {\\n                        const subLine = lines[i];\\n                        if (!subLine.startsWith('-')) continue;\\n\\n                        const subMatch = /-\\\\s*(.*?)\\\\s*:(.*)/.exec(subLine);\\n                        if (subMatch) {\\n                            const subName = subMatch[1].trim();\\n                            const charactersText = subMatch[2].trim();\\n                            const characters = charactersText ? charactersText.split(',').map(c => c.trim()).filter(c => c) : [];\\n                            console.log(`  子地点: ${subName}, 角色: ${characters.join(', ') || '(无)'}`);\\n                            subLocations.push({ name: subName, characters: characters });\\n                        } else {\\n                             const subMatchNoColon = /-\\\\s*(.*)/.exec(subLine);\\n                             if(subMatchNoColon) {\\n                                const subName = subMatchNoColon[1].trim();\\n                                console.log(`  子地点: ${subName}, 角色: (无)`);\\n                                subLocations.push({ name: subName, characters: [] });\\n                             }\\n                        }\\n                    }\\n\\n                    if (mainLocation && !mainLocation.startsWith('MOVEBLOCK:')) {\\n                        data.locations.push({ name: mainLocation, subLocations: subLocations });\\n                    }\\n                }\\n\\n                console.log(\\\"解析后的地图数据 (moveBlocked将始终为false):\\\", data);\\n                if(data.locations.length === 0 && text.includes('[')){\\n                    console.warn(\\\"警告：原始文本包含 '[' 但未能解析出任何地点数据。请检查数据格式。\\\");\\n                }\\n                return data;\\n            } catch (error) {\\n                console.error(\\\"解析地图数据时出错:\\\", error);\\n                return null;\\n            }\\n        }\\n\\n        // 渲染地图界面\\n        function renderMapInterface(mapData) {\\n             // (之前的检查逻辑保持不变)\\n            if (!mapData || !mapData.locations) {\\n                 console.error(\\\"渲染地图失败：无效的 mapData 或缺少 locations 数组\\\", mapData);\\n                 const mainLocationsContainer = document.getElementById('main-locations');\\n                 mainLocationsContainer.innerHTML = '<p style=\\\"color: red; text-align: center;\\\">地图数据加载失败，请检查格式或联系开发者。</p>';\\n                 document.getElementById('sub-locations').classList.add('hidden');\\n                 document.getElementById('location-details').classList.add('hidden');\\n                 document.getElementById('movement-alert').classList.add('hidden');\\n                 return;\\n            }\\n\\n            // START OF CHANGE - hasMoveBlock 将总是 false\\n            hasMoveBlock = mapData.moveBlocked; // 这里获取到的值将总是 false\\n            // END OF CHANGE\\n\\n            // 显示/隐藏移动限制提示\\n            const movementAlert = document.getElementById('movement-alert');\\n            // 因为 hasMoveBlock 总是 false，所以下面的 if 永远不会执行，alert 会一直隐藏\\n            if (hasMoveBlock) {\\n                movementAlert.classList.remove('hidden');\\n            } else {\\n                movementAlert.classList.add('hidden');\\n            }\\n\\n            // 渲染主地点按钮\\n            const mainLocationsContainer = document.getElementById('main-locations');\\n            mainLocationsContainer.innerHTML = '';\\n\\n             if (mapData.locations.length === 0) {\\n                mainLocationsContainer.innerHTML = '<p style=\\\"color: var(--gal-text-muted); text-align: center;\\\">当前没有可用的地点信息。</p>';\\n                document.getElementById('sub-locations').classList.add('hidden');\\n                document.getElementById('location-details').classList.add('hidden');\\n                return;\\n            }\\n\\n            mapData.locations.forEach(location => {\\n                 if (!location || typeof location.name !== 'string') {\\n                    console.warn(\\\"跳过无效的地点数据:\\\", location);\\n                    return;\\n                 }\\n\\n                const button = document.createElement('button');\\n                button.className = 'location-button';\\n\\n                let icon = 'fa-map';\\n                 if (location.name.includes('学校') || location.name.includes('校区') || location.name.includes('教学')) icon = 'fa-school';\\n                 else if (location.name.includes('公园') || location.name.includes('森林') || location.name.includes('花园')) icon = 'fa-tree';\\n                 else if (location.name.includes('商业') || location.name.includes('市场') || location.name.includes('商店') || location.name.includes('街')) icon = 'fa-store';\\n                 else if (location.name.includes('住宅') || location.name.includes('家') || location.name.includes('公寓')) icon = 'fa-home';\\n                 else if (location.name.includes('站') || location.name.includes('车站') || location.name.includes('机场')) icon = 'fa-train';\\n                 else if (location.name.includes('行政') || location.name.includes('办公')) icon = 'fa-building'; // 例如行政楼\\n                 else if (location.name.includes('监狱') || location.name.includes('禁闭')) icon = 'fa-gavel'; // 例如监狱相关\\n\\n                button.innerHTML = `<i class=\\\"fas ${icon}\\\"></i> ${location.name}`;\\n                button.dataset.location = location.name;\\n\\n                button.addEventListener('click', () => selectMainLocation(location));\\n\\n                // 因为 hasMoveBlock 总是 false，下面的 if 永远不会执行，按钮不会被禁用\\n                if (hasMoveBlock) {\\n                    button.disabled = true;\\n                    button.classList.add('disabled');\\n                }\\n\\n                mainLocationsContainer.appendChild(button);\\n            });\\n\\n            document.getElementById('sub-locations').classList.add('hidden');\\n            document.getElementById('location-details').classList.add('hidden');\\n        }\\n\\n        // 选择主地点\\n        function selectMainLocation(location) {\\n            // 因为 hasMoveBlock 总是 false, 这个检查永远不会阻止执行\\n            if (hasMoveBlock || !location || !location.subLocations) return;\\n\\n            selectedMain = location;\\n            selectedSub = null;\\n            document.getElementById('location-details').classList.add('hidden');\\n\\n            const allMainButtons = document.querySelectorAll('#main-locations .location-button');\\n            allMainButtons.forEach(btn => {\\n                btn.classList.remove('selected');\\n                if (btn.dataset.location === location.name) {\\n                    btn.classList.add('selected');\\n                }\\n            });\\n\\n            const subLocationsContainer = document.getElementById('sub-locations');\\n            subLocationsContainer.innerHTML = '';\\n\\n            if (location.subLocations.length === 0) {\\n                 subLocationsContainer.innerHTML = '<p style=\\\"color: var(--gal-text-muted); text-align: center; padding: 10px 0;\\\">此区域下没有具体的探索地点。</p>';\\n                 subLocationsContainer.classList.remove('hidden');\\n                 return;\\n            }\\n\\n            location.subLocations.forEach(subLocation => {\\n                if (!subLocation || typeof subLocation.name !== 'string') {\\n                    console.warn(\\\"跳过无效的子地点数据:\\\", subLocation);\\n                    return;\\n                }\\n\\n                const button = document.createElement('button');\\n                button.className = 'sub-location-button';\\n\\n                 let icon = 'fa-map-pin';\\n                 if (subLocation.name.includes('教室')) icon = 'fa-chalkboard';\\n                 else if (subLocation.name.includes('办公室')) icon = 'fa-briefcase';\\n                 else if (subLocation.name.includes('档案室')) icon = 'fa-archive';\\n                 else if (subLocation.name.includes('审讯室') || subLocation.name.includes('审问室')) icon = 'fa-gavel';\\n                 else if (subLocation.name.includes('走廊') || subLocation.name.includes('通道')) icon = 'fa-road';\\n                 else if (subLocation.name.includes('咖啡') || subLocation.name.includes('茶馆')) icon = 'fa-coffee';\\n                 else if (subLocation.name.includes('图书') || subLocation.name.includes('阅览室')) icon = 'fa-book';\\n                 else if (subLocation.name.includes('商店') || subLocation.name.includes('超市') || subLocation.name.includes('便利店')) icon = 'fa-shopping-bag';\\n                 else if (subLocation.name.includes('广场') || subLocation.name.includes('中心')) icon = 'fa-landmark';\\n                 else if (subLocation.name.includes('房间') || subLocation.name.includes('卧室') || subLocation.name.includes('宿舍')) icon = 'fa-bed';\\n                 else if (subLocation.name.includes('厨房') || subLocation.name.includes('餐厅')) icon = 'fa-utensils';\\n                 else if (subLocation.name.includes('仓库')) icon = 'fa-warehouse';\\n                 else if (subLocation.name.includes('天台') || subLocation.name.includes('屋顶')) icon = 'fa-building';\\n\\n\\n                button.innerHTML = `<i class=\\\"fas ${icon}\\\"></i> ${subLocation.name}`;\\n                button.dataset.sublocation = subLocation.name;\\n\\n                button.addEventListener('click', () => selectSubLocation(subLocation));\\n\\n                subLocationsContainer.appendChild(button);\\n            });\\n\\n            subLocationsContainer.classList.remove('hidden');\\n        }\\n\\n        // 选择子地点\\n        function selectSubLocation(subLocation) {\\n            // if (hasMoveBlock) return; // 这个检查不再必要，因为总是 false\\n            if (!subLocation || !subLocation.characters || !selectedMain) return;\\n\\n            selectedSub = subLocation;\\n            selectedCharacters = subLocation.characters;\\n\\n            const allSubButtons = document.querySelectorAll('#sub-locations .sub-location-button');\\n            allSubButtons.forEach(btn => {\\n                btn.classList.remove('selected');\\n                if (btn.dataset.sublocation === subLocation.name) {\\n                    btn.classList.add('selected');\\n                }\\n            });\\n\\n            document.getElementById('selected-location').textContent = `${selectedMain.name} - ${subLocation.name}`;\\n\\n            const charactersContainer = document.getElementById('location-characters');\\n            charactersContainer.innerHTML = '';\\n\\n            if (selectedCharacters.length > 0) {\\n                selectedCharacters.forEach(character => {\\n                    const charElement = document.createElement('div');\\n                    charElement.className = 'character-item';\\n                    const textNode = document.createTextNode(` ${character}`);\\n                    const iconElement = document.createElement('i');\\n                    iconElement.className = 'fas fa-user';\\n                    charElement.appendChild(iconElement);\\n                    charElement.appendChild(textNode);\\n                    charactersContainer.appendChild(charElement);\\n                });\\n            } else {\\n                const noCharElement = document.createElement('div');\\n                noCharElement.className = 'character-item empty';\\n                noCharElement.innerHTML = `<i class=\\\"fas fa-user-slash\\\"></i> 这里没有人`;\\n                charactersContainer.appendChild(noCharElement);\\n            }\\n\\n            document.getElementById('location-details').classList.remove('hidden');\\n        }\\n\\n        // 确认移动\\n        function confirmMove() {\\n            // 因为 hasMoveBlock 总是 false, 这个检查永远不会阻止执行\\n            if (hasMoveBlock || !selectedMain || !selectedSub) {\\n                console.log(\\\"确认移动失败：未选择完整地点\\\"); // 移除了移动阻止的日志\\n                return;\\n            }\\n\\n            const locationName = `${selectedMain.name} - ${selectedSub.name}`;\\n            const characters = Array.isArray(selectedCharacters) ? selectedCharacters.join(', ') : '';\\n\\n            const confirmMessage = document.getElementById('confirm-message');\\n            if (characters) {\\n                confirmMessage.textContent = `前往 ${locationName}？你可能会遇见：${characters}`;\\n            } else {\\n                confirmMessage.textContent = `前往 ${locationName}？那里好像没有人。`;\\n            }\\n\\n            document.getElementById('confirm-overlay').classList.remove('hidden');\\n        }\\n\\n        // 执行移动\\n        function executeMove() {\\n            // 因为 hasMoveBlock 总是 false, 这个检查永远不会阻止执行\\n            if (hasMoveBlock || !selectedMain || !selectedSub) {\\n                 console.log(\\\"执行移动失败：未选择完整地点\\\"); // 移除了移动阻止的日志\\n                 return;\\n            }\\n\\n            const locationName = `${selectedMain.name} - ${selectedSub.name}`;\\n            const characters = Array.isArray(selectedCharacters) ? selectedCharacters.join(',') : '';\\n\\n            const cleanLocationName = locationName.replace(/[<>]/g, '');\\n            const cleanCharacters = characters.replace(/[<>]/g, '');\\n\\n            let message = `<request:{{user}}前往了${cleanLocationName}.`;\\n            if (cleanCharacters) {\\n                message += `${cleanCharacters}在那里`;\\n            }\\n            message += \\\">\\\";\\n\\n            console.log(\\\"准备发送移动命令:\\\", message);\\n\\n            if (typeof triggerSlash === 'function') {\\n                try {\\n                    console.log(\\\"检测到 triggerSlash 函数，尝试发送命令...\\\");\\n                    triggerSlash(`/send ${message} || /trigger`);\\n                    console.log(\\\"命令已发送至 triggerSlash\\\");\\n                } catch (e) {\\n                    console.error(\\\"通过 triggerSlash 发送消息时出错:\\\", e);\\n                    alert(\\\"发送移动指令失败，请检查控制台信息或重试。\\\");\\n                }\\n            } else {\\n                console.warn(\\\"未检测到 triggerSlash 函数 (可能不在SillyTavern环境中)。\\\");\\n                console.log(\\\"模拟移动命令:\\\", message);\\n                alert(`移动请求已生成 (仅供测试):\\\\n${message}\\\\n\\\\n在实际SillyTavern环境中，这将自动发送。`);\\n            }\\n\\n            document.getElementById('confirm-overlay').classList.add('hidden');\\n            resetSelection();\\n        }\\n\\n        // 重置选择\\n        function resetSelection() {\\n            selectedMain = null;\\n            selectedSub = null;\\n            selectedCharacters = [];\\n\\n            document.querySelectorAll('.location-button.selected, .sub-location-button.selected').forEach(btn => {\\n                btn.classList.remove('selected');\\n            });\\n\\n            document.getElementById('sub-locations').classList.add('hidden');\\n            document.getElementById('location-details').classList.add('hidden');\\n            const selectedLocationSpan = document.getElementById('selected-location');\\n             if(selectedLocationSpan) {\\n                 selectedLocationSpan.textContent = '未选择';\\n             }\\n        }\\n\\n        // 处理地图数据 (修改版：不再检查父窗口/URL，不再强制设置moveBlocked=true)\\n        function processMapData() {\\n            const dataSource = document.getElementById('map-data-source');\\n            if (!dataSource) {\\n                console.error(\\\"错误：未找到地图数据源元素 #map-data-source\\\");\\n                 renderMapInterface(null);\\n                return;\\n            }\\n\\n            let mapText = dataSource.textContent || dataSource.innerText || '';\\n            console.log(\\\"从 #map-data-source 获取到的地图数据文本:\\\", mapText ? `有数据 (长度: ${mapText.length})` : \\\"无数据\\\");\\n\\n            let useTestData = false;\\n             if (!mapText || mapText.trim() === '') {\\n                 console.log(\\\"数据源为空，准备使用测试数据。\\\");\\n                 useTestData = true;\\n             // 保留对标记的检查，如果连标记都没有，也用测试数据\\n             } else if (!mapText.includes('[MOVEBLOCK:') && !mapText.includes('[EMOTION:')) {\\n                 console.log(\\\"数据源内容不包含地图标记 [MOVEBLOCK:] 或 [EMOTION:]，准备使用测试数据。\\\");\\n                 useTestData = true;\\n             }\\n\\n\\n            if (useTestData) {\\n                console.log(\\\"使用内置测试数据...\\\");\\n                // 测试数据现在默认 MOVEBLOCK:NO\\n                mapText = `[MOVEBLOCK:NO]\\n[校园区域\\n- 教学楼: 班主任, 数学老师\\n- 图书馆: 文学社社长, 图书管理员\\n- 操场: 体育老师, 田径队队长]\\n[商业街\\n- 咖啡厅: 服务员小雅, 钢琴少女\\n- 游戏厅: 游戏高手小明, 不良少年\\n- 书店: 眼镜店主, 神秘顾客]\\n[住宅区\\n- 公寓楼: 邻居阿姨, 宠物猫咪\\n- 小公园: 晨练老人, 遛狗少年\\n- 便利店: 店长, 兼职学生]`;\\n                console.log(\\\"已设置测试数据。\\\");\\n            }\\n\\n            // 兼容旧版数据格式（EMOTION -> MOVEBLOCK）\\n            // 保留此逻辑，以防输入旧格式数据\\n            if (mapText && mapText.includes('[EMOTION:')) {\\n                console.log(\\\"检测到旧版数据格式，执行转换 [EMOTION:] -> [MOVEBLOCK:]\\\");\\n                mapText = mapText.replace(/\\\\[EMOTION:(BAD|GOOD)\\\\]/g, function(match, state) {\\n                    // 即使转换，最终也会被 parseMapData 忽略 YES 状态\\n                    const newState = state === 'BAD' ? 'YES' : 'NO';\\n                    console.log(`  转换: ${match} -> [MOVEBLOCK:${newState}] (注意: YES状态将被忽略)`);\\n                    return `[MOVEBLOCK:${newState}]`;\\n                });\\n            }\\n\\n            console.log(\\\"最终用于解析的 mapText:\\\", mapText.substring(0, 100) + \\\"...\\\");\\n            mapData = parseMapData(mapText); // 解析数据，此时 mapData.moveBlocked 将为 false\\n\\n            renderMapInterface(mapData); // 渲染，此时 hasMoveBlock 会被设为 false\\n\\n            if (!mapData) {\\n                console.error(\\\"地图数据解析最终失败，请检查传入的地图数据格式是否正确。\\\");\\n            } else if (mapData.locations.length === 0 && mapText.includes('[')) {\\n                 console.warn(\\\"地图数据解析完成，但未找到任何有效地点。原始数据可能格式有误或内容为空。\\\");\\n            } else {\\n                 console.log(\\\"地图数据解析和渲染流程完成。\\\");\\n            }\\n        }\\n\\n        // 注册事件监听器\\n        function setupEventListeners() {\\n            document.getElementById('confirm-move-btn').addEventListener('click', confirmMove);\\n            document.getElementById('cancel-move-btn').addEventListener('click', () => {\\n                document.getElementById('confirm-overlay').classList.add('hidden');\\n            });\\n            document.getElementById('execute-move-btn').addEventListener('click', executeMove);\\n             document.getElementById('confirm-overlay').addEventListener('click', (event) => {\\n                if (event.target === event.currentTarget) {\\n                    document.getElementById('confirm-overlay').classList.add('hidden');\\n                }\\n             });\\n        }\\n\\n        // 初始化\\n        function init() {\\n            console.log(\\\"地图模块初始化开始 (已禁用移动限制功能)...\\\");\\n            setupEventListeners();\\n            processMapData();\\n\\n             if (window.parent && window.parent !== window && typeof window.parent.eventOn === 'function' && window.parent.tavern_events) {\\n                console.log(\\\"检测到 SillyTavern 环境，设置消息更新监听器...\\\");\\n                const debounceProcessMapData = debounce(processMapData, 300);\\n\\n                window.parent.eventOn(window.parent.tavern_events.MESSAGE_UPDATED, () => {\\n                    console.log(\\\"触发事件：MESSAGE_UPDATED，延时处理地图数据...\\\");\\n                    debounceProcessMapData();\\n                });\\n                window.parent.eventOn(window.parent.tavern_events.MESSAGE_RECEIVED, () => {\\n                    console.log(\\\"触发事件：MESSAGE_RECEIVED，延时处理地图数据...\\\");\\n                   debounceProcessMapData();\\n                });\\n                console.log(\\\"消息更新监听器设置完毕。\\\");\\n            } else {\\n                 console.log(\\\"未检测到 SillyTavern 环境或事件监听不可用。\\\");\\n            }\\n            console.log(\\\"地图模块初始化完成。\\\");\\n        }\\n\\n        // 防抖函数\\n        function debounce(func, wait) {\\n            let timeout;\\n            return function executedFunction(...args) {\\n                const later = () => {\\n                    clearTimeout(timeout);\\n                    func.apply(this, args);\\n                };\\n                clearTimeout(timeout);\\n                timeout = setTimeout(later, wait);\\n            };\\n        }\\n\\n        document.addEventListener('DOMContentLoaded', init);\\n    </script>\\n\\n    <!-- CSS 样式部分保持不变 -->\\n    <style>\\n        @import url('https://fonts.googleapis.com/css2?family=ZCOOL+XiaoWei&family=ZCOOL+QingKe+HuangYou&display=swap');\\n\\n        :root {\\n            /* 莫兰迪色系 */\\n            --gal-bg-light: #f8f4f0;      /* 背景 - 极浅米白 */\\n            --gal-bg-medium: #e8e2d9;     /* 中等背景 - 浅灰褐色 */\\n            --gal-border: #d3c6b9;       /* 边框/点缀 - 柔和米棕 */\\n            --gal-text-dark: #6b5f56;     /* 深色文字 - 柔和深棕 */\\n            --gal-text-muted: #a39589;    /* 浅色文字 - 中灰棕 */\\n            --gal-primary: #b4a295;       /* 主色 - 灰玫瑰棕 */\\n            --gal-primary-light: #dcd1c7; /* 浅主色 */\\n            --gal-success: #a5bdaa;       /* 成功 - 柔和绿 */\\n            --gal-warning: #e5d7b3;       /* 警告 - 柔和黄 */\\n            --gal-danger: #d8a097;        /* 危险 - 柔和粉红 */\\n            --gal-pink: #e6c6c6;          /* 粉色 - 柔和粉 */\\n            --gal-purple: #c8b9d3;        /* 紫色 - 柔和薰衣草紫 */\\n\\n            --font-title: 'ZCOOL QingKe HuangYou', sans-serif;\\n            --font-main: 'ZCOOL XiaoWei', \\\"Microsoft YaHei\\\", sans-serif;\\n\\n            /* Material Design 风格 */\\n            --md-border-radius-large: 20px; /* 大圆角 */\\n            --md-border-radius-medium: 16px;/* 中圆角 */\\n            --md-border-radius-small: 12px; /* 小圆角 */\\n            --md-shadow-1: 0 2px 4px rgba(0, 0, 0, 0.08); /* 细微阴影 */\\n            --md-shadow-2: 0 4px 8px rgba(0, 0, 0, 0.1);  /* 轻度阴影 */\\n            --md-shadow-3: 0 6px 12px rgba(0, 0, 0, 0.12); /* 中度阴影 */\\n            --md-shadow-hover: 0 8px 16px rgba(0, 0, 0, 0.15); /* 悬停阴影 */\\n        }\\n\\n        body {\\n            background-color: transparent; /* or var(--gal-bg-light) if not transparent */\\n            margin: 0;\\n            padding: 10px; /* Add some padding around the body */\\n            font-family: var(--font-main);\\n            color: var(--gal-text-dark);\\n            box-sizing: border-box;\\n        }\\n\\n        /* 地图界面容器 */\\n        .map-interface {\\n            max-width: 100%;\\n            width: 100%;\\n            margin: 0 auto;\\n            background: var(--gal-bg-light);\\n            border: 1px solid var(--gal-border); /* Thinner border */\\n            border-radius: var(--md-border-radius-large); /* 大圆角 */\\n            box-shadow: var(--md-shadow-2); /* 应用阴影 */\\n            padding: 25px;\\n            box-sizing: border-box;\\n            position: relative;\\n            overflow: hidden; /* 防止内容溢出圆角 */\\n             min-height: 200px; /* 保证即使没内容也有最小高度 */\\n        }\\n\\n        /* 地图标题 */\\n        .map-header {\\n            text-align: center;\\n            margin-bottom: 25px;\\n            padding-bottom: 15px;\\n            border-bottom: 1px solid var(--gal-border); /* 更细的分割线 */\\n        }\\n\\n        .map-header h2 {\\n            font-size: 26px; /* Slightly smaller */\\n            margin: 0;\\n            padding: 5px;\\n            line-height: 1.4;\\n            color: var(--gal-primary);\\n            font-family: var(--font-title);\\n            position: relative;\\n        }\\n\\n        .map-header h2:before,\\n        .map-header h2:after {\\n            content: '✧'; /* Changed icon */\\n            display: inline-block;\\n            margin: 0 10px;\\n            color: var(--gal-pink);\\n            font-size: 18px;\\n            opacity: 0.8;\\n        }\\n\\n        /* 移动限制提示 (样式保留，但会被JS隐藏) */\\n        .movement-alert {\\n            background-color: var(--gal-warning);\\n            border: 1px solid rgba(229, 215, 179, 0.8); /* Lighter border */\\n            border-radius: var(--md-border-radius-medium); /* 圆角 */\\n            padding: 15px;\\n            margin-bottom: 25px;\\n            text-align: center;\\n            color: var(--gal-text-dark);\\n            font-weight: normal; /* Less emphasis */\\n            box-shadow: var(--md-shadow-1);\\n            animation: pulse-movement 2.5s infinite ease-in-out; /* Smoother pulse */\\n            /* 默认隐藏，由JS移除 hidden 类来显示 */\\n             /* 