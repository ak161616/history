SendButton.click();\\n                        } else {\\n                             console.error(`Fallback failed: Could not find textarea (tried #send_textarea, #prompt-input: ${!!targetTextarea}) or send button (tried #send_but, #send_button, button[type=submit]: ${!!targetSendButton}) in parent document.`);\\n                             if(targetTextarea) {\\n                                 console.log('Fallback: Found textarea, attempting to fill value only with:', requestText);\\n                                 targetTextarea.value = requestText;\\n                                 targetTextarea.dispatchEvent(new Event('input', { bubbles: true }));\\n                             }\\n                             // Fallback alert if sending fails\\n                             alert(`已选择: ${currentProfile.name}\\\\n简介: ${currentProfile.bio} (发送机制未找到)`);\\n                        }\\n                    } else {\\n                         console.error('Fallback failed: Cannot access parent window document.');\\n                         alert(`已选择: ${currentProfile.name}\\\\n简介: ${currentProfile.bio} (无法访问父窗口)`);\\n                    }\\n                }\\n            } catch (error) {\\n                console.error('Error sending choice via button:', error);\\n                alert(`已选择: ${currentProfile.name}\\\\n简介: ${currentProfile.bio} (发送时发生错误: ${error.message})`);\\n            }\\n        });\\n\\n        nextProfileButton.addEventListener('click', () => {\\n            if (profiles.length === 0) return;\\n            currentProfileIndex = Math.floor(Math.random() * profiles.length); // Always pick random\\n            displayProfile(profiles[currentProfileIndex]);\\n        });\\n\\n        // if (viewDetailsButton) { // Button is removed, so this block is no longer needed\\n        //     viewDetailsButton.addEventListener('click', () => {\\n        //         if (!profiles.length || currentProfileIndex < 0 || currentProfileIndex >= profiles.le