// if (viewDetailsButton) viewDetailsButton.style.display = 'block'; // Button is removed\\n\\n            profileAvatarImg.src = profile.avatarUrl || '';\\n            profileAvatarImg.alt = profile.name ? `头像: ${profile.name}` : '头像';\\n            profileNameDisplay.textContent = profile.name || '未知姓名';\\n            profileTitleDisplay.textContent = profile.title || '暂无描述';\\n            \\n            currentProfileBio = profile.bio || '暂无简介';\\n            currentProfileStory = profile.story || ''; // Story is now empty\\n\\n            profileInfoDisplay.textContent = currentProfileBio; // Display bio initially\\n            isShowingStory = false; // Reset state\\n            // if (viewDetailsButton) viewDetailsButton.textContent = '查看详情'; // Button is removed\\n        }\\n\\n        initialShakeButton.addEventListener('click', () => {\\n            shakeScreen.style.display = 'none';\\n            profileScreen.style.display = 'flex';\\n            if (profiles.length > 0) {\\n                currentProfileIndex = Math.floor(Math.random() * profiles.length);\\n                displayProfile(profiles[currentProfileIndex]);\\n            } else {\\n                // Handle case with no profiles\\n                displayProfile(null); // Let displayProfile handle UI for no profile\\n            }\\n        });\\n\\n        chooseHerButton.addEventListener('click', () => {\\n            if (profiles.length === 0 || !profiles[currentProfileIndex]) return;\\n            const currentProfile = profiles[currentProfileIndex];\\n            \\n            // 构建要发送的文本\\n            // 将简介和剧情梗概中的换行符和多余空格处理掉，以避免在 requestText 中产生问题\\n            const cleanName = currentProfile.name.replace(/\\\\s+/g, ' ').trim();\\n            const cleanTitle = currentProfile.title.replace(/\\\\s+/g, ' ').trim();\\n            const cleanBio = currentProfile.bio.replace(/\\\\s+/g, ' ').trim();\\n            // const cleanStory = currentProfile.story.replace(/\\\\s+/g, ' ').trim(); /