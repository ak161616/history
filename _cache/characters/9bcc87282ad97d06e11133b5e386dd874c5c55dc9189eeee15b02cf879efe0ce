     customInputElement.style.display = 'none';\\n                    customInputElement.value = '';\\n                }\\n            }\\n        }\\n\\n        document.addEventListener('DOMContentLoaded', function() {\\n            const legends = document.querySelectorAll('.profile-container fieldset > legend');\\n            legends.forEach(legend => {\\n                const contentWrapper = legend.nextElementSibling;\\n                const icon = legend.querySelector('.toggle-icon');\\n                if (contentWrapper && contentWrapper.classList.contains('fieldset-content-wrapper') && icon) {\\n                    contentWrapper.classList.add('collapsed');\\n                    icon.textContent = icon.dataset.collapsedIcon || '►';\\n                    legend.addEventListener('click', function() {\\n                        const isCollapsed = contentWrapper.classList.toggle('collapsed');\\n                        icon.textContent = isCollapsed ? (icon.dataset.collapsedIcon || '►') : (icon.dataset.expandedIcon || '▼');\\n                    });\\n                }\\n            });\\n\\n            document.getElementById('charAffiliation').addEventListener('change', () => handleCustomInput('charAffiliation', 'charAffiliation-custom'));\\n            document.getElementById('powerLevel').addEventListener('change', () => handleCustomInput('powerLevel', 'powerLevel-custom'));\\n            document.getElementById('initialLocation').addEventListener('change', () => handleCustomInput('initialLocation', 'initialLocation-custom'));\\n            document.getElementById('speciesEarthRelation').addEventListener('change', () => handleCustomInput('speciesEarthRelation', 'speciesEarthRelation-custom'));\\n\\n\\n            document.querySelectorAll('input[name=\\\"charStatus\\\"]').forEach(radio => {\\n                radio.addEventListener('change', updateFieldVisibility);\\n            });\\n\\n            updateFieldVisibility();\\n            handleCustomInput('charAffiliation', 'charAffiliation-custom');\\n            handleCustomInput('powerLevel', 'powerLevel-custom');\\n            handleCustomInput('initialLocation', 'initialLocation-custom');\\n            handleCustomInput('speciesEarthRelation', 'speciesEarthRelation-custom');\\n        });\\n\\n        function updateFieldVisibility() {\\n            const statusElement = document.querySelector('input[name=\\\"charStatus\\\"]:checked');\\n            if (!statusElement) return;\\n            const status = statusElement.value;\\n\\n            const superpoweredFieldsDiv = document.getElementById('superpoweredFieldsWrapper');\\n            const otherSpeciesFieldsDiv = document.getElementById('otherSpeciesFieldsWrapper');\\n\\n            const fieldsToMakeRequiredForSuperpowered = ['innateTalents', 'superpowers', 'powerSource'];\\n            const fieldsToMakeRequiredForOtherSpecies = ['speciesName'];\\n\\n            fieldsToMakeRequiredForSuperpowered.forEach(id => {\\n                const el = document.getElementById(id);\\n                if (el) el.required = false;\\n            });\\n            fieldsToMakeRequiredForOtherSpecies.forEach(id => {\\n                const el = document.getElementById(id);\\n                if (el) el.required = false;\\n            });\\n            const powerLevelSelect = document.getElementById('powerLevel');\\n            if (powerLevelSelect) powerLevelSelect.required = false;\\n\\n\\n            if (status === 'ordinary') {\\n                superpoweredFieldsDiv.classList.remove('visible-fields');\\n                superpoweredFieldsDiv.classList.add('hidden-fields');\\n                otherSpeciesFieldsDiv.classList.remove('visible-fields');\\n                otherSpeciesFieldsDiv.classList.add('hidden-fields');\\n            }\\n            else if (status === 'superpowered') {\\n                superpoweredFieldsDiv.classList.remove('hidden-fields');\\n                superpoweredFieldsDiv.classList.add('visible-fields');\\n                otherSpeciesFieldsDiv.classList.remove('visible-fields');\\n                otherSpeciesFieldsDiv.classList.add('hidden-fields');\\n                fieldsToMakeRequiredForSuperpowered.forEach(id => {\\n                    const el = document.getElementById(id);\\n                    if (el) el.required = true;\\n                });\\n            }\\n            else if (status === 'other_species') {\\n                otherSpeciesFieldsDiv.classList.remove('hidden-fields');\\n                otherSpeciesFieldsDiv.classList.add('visible-fields');\\n                superpoweredFieldsDiv.classList.remove('visible-fields');\\n                superpoweredFieldsDiv.classList.add('hidden-fields');\\n                fieldsToMakeRequiredForOtherSpecies.forEach(id => {\\n                    const el = document.getElementById(id);\\n                    if (el) el.required = true;\\n                });\\n            }\\n        }\\n\\n\\n        function getInputTextValue(elementId, isRequired = false, fieldName = \\\"字段\\\") {\\n            const element = document.getElementById(elementId);\\n            if (!element) {\\n                console.error(`Element with ID ${elementId} not found.`);\\n                return `[${fieldName.toUpperCase()}_配置错误]`;\\n            }\\n            let value = element.value.trim();\\n            if (value === '') {\\n                return isRequired ? `[${fieldName.toUpperCase()}_关键数据缺失]` : '随机';\\n            }\\n            return value;\\n        }\\n\\n        function getSelectOrCustomValue(selectId, customInputId, isRequired = false, fieldName = \\\"字段\\\") {\\n            const selectElement = document.getElementById(selectId);\\n            const customInputElement = document.getElementById(customInputId);\\n            let value = \\\"\\\";\\n\\n            if (!selectElement) {\\n                console.error(`Element with ID ${selectId} not found.`);\\n                return `[${fieldName.toUpperCase()}_选择框配置错误]`;\\n            }\\n\\n            if (selectElement.value === \\\"自定义\\\") {\\n                if (!customInputElement) {\\n                    console.error(`Element with ID ${customInputElementId} not found for select ${selectId}.`);\\n                    return `[${fieldName.toUpperCase()}_自定义输入配置错误]`;\\n                }\\n                value = customInputElement.value.trim();\\n            } else {\\n                value = selectElement.value;\\n            }\\n\\n            if (value === \\\"\\\" || value === \\\"请选择或自定义...\\\") {\\n                if (isRequired) {\\n                    return `[${fieldName.toUpperCase()}_关键数据缺失]`;\\n                } else {\\n                    return \\\"随机\\\";\\n                }\\n            }\\n            return value;\\n        }\\n\\n        function clearForm() {\\n            const form = document.getElementById('characterForm');\\n            form.reset();\\n\\n            document.querySelectorAll('.custom-input').forEach(input => {\\n                input.style.display = 'none';\\n            });\\n\\n            const defaultStatus = form.querySelector('input[name=\\\"charStatus\\\"][value=\\\"ordinary\\\"]');\\n            if (defaultStatus) defaultStatus.checked = true;\\n            updateFieldVisibility();\\n\\n            const legends = document.querySelectorAll('.profile-container fieldset > legend');\\n            legends.forEach(legend => {\\n                const contentWrapper = legend.nextElementSibling;\\n                const icon = legend.querySelector('.toggle-icon');\\n                if (contentWrapper && contentWrapper.classList.contains('fieldset-content-wrapper') && icon) {\\n                    if (!contentWrapper.classList.contains('collapsed')) {\\n                        contentWrapper.classList.add('collapsed');\\n                        icon.textContent = icon.dataset.collapsedIcon || '►';\\n                    }\\n                }\\n            });\\n            han