lue;\\n    }\\n\\n    function deepGet(obj, path) {\\n        if (typeof path !== 'string') return undefined;\\n        return path.split('.').reduce((prev, curr) => {\\n            return prev ? prev[curr] : undefined;\\n        }, obj);\\n    }\\n\\n    function updateProgressBar(barId, value) {\\n        let numericValue = 0;\\n        let displayValue = SafeGetValue(value);\\n\\n        if (typeof displayValue === 'string' && displayValue.includes('->')) {\\n            const parts = displayValue.split('->');\\n            numericValue = parseFloat(parts[parts.length - 1].trim());\\n        } else {\\n            numericValue = parseFloat(displayValue);\\n        }\\n\\n        const progressBar = document.getElementById(barId);\\n        if(!progressBar) return;\\n\\n        const min = -50;\\n        const max = 200;\\n        const percentage = ((numericValue - min) / (max - min)) * 100;\\n        progressBar.style.width = `${Math.max(0, Math.min(100, percentage))}%`;\\n\\n        document.getElementById('qiunan-affection').innerText = displayValue;\\n    }\\n\\n    function addEventMask(eventId, eventValue) {\\n        const eventProperty = document.getElementById(`${eventId}-property`);\\n        if (!eventProperty) return;\\n\\n        const existingMask = eventProperty.querySelector('.event-mask');\\n        if (existingMask) {\\n            existingMask.remove();\\n        }\\n\\n        const value = SafeGetValue(eventValue);\\n\\n        if (value === 'æœªè§¦å‘') {\\n            const mask = document.createElement('div');\\n            mask.className = 'event-mask';\\n            let maskText = '';\\n            let maskClass = '';\\n\\n            if (['event-childhood', 'event-memory', 'event-trauma', 'event-fall'].includes(eventId)) {\\n                maskText = 'ç§˜å¯†';\\n                maskClass = 'secret-events';\\n            } else if (['event-nanga', 'event-erhai'].includes(eventId)) {\\n                maskText = 'æœªæ¥';\\n                maskClass = 'future-events';\\n            } else if (eventId === 'event-separation') {\\n                maskText = 'æ‚”æ¨çš„æŠ‰æ‹©';\\n                maskClass = 'regret-events';\\n            }\\n\\n            if (maskText) {\\n                mask.classList.add(maskClass);\\n                mask.innerHTML = `<div class=\\\"event-mask-text ${maskClass}\\\">${maskText}</div>`;\\n                eventProperty.appendChild(mask);\\n            }\\n        }\\n    }\\n\\n    function updateDisplay() {\\n        // This function now uses the global characterData which is set by updateDataSource\\n        const showDeltaOnly = document.getElementById('data-toggle').checked;\\n        const allProperties = document.querySelectorAll('.property');\\n        const allSections = document.querySelectorAll('.section');\\n\\n        allProperties.forEach(p => p.classList.add('hidden'));\\n        allSections.forEach(s => s.classList.add('hidden'));\\n\\n        const updateElement = (elementId, value) => {\\n            const element = document.getElementById(elementId);\\n            if (element) {\\n                element.innerText = SafeGetValue(value);\\n                const propertyDiv = element.closest('.property');\\n                const sectionDiv = element.closest('.section');\\n                if (propertyDiv) propertyDiv.classList.remove('hidden');\\n                if (sectionDiv) sectionDiv.classList.remove('hidden');\\n            }\\n        };\\n\\n        const updateEventElement = (eventId, value) => {\\n            const propElement = document.getElementById(`${eventId}-property`);\\n            if (propElement) {\\n                document.getElementById(eventId).innerText = SafeGetValue(value);\\n                addEventMask(eventId, value);\\n                propElement.classList.remove('hidden');\\n                propElement.closest('.section').classList.remove('hidden');\\n            }\\n        };\\n\\n        const checkAndUpdate = (path, eleme