\\n                                        sentIndicator.style.zIndex = '10';\\n                                        sentIndicator.style.opacity = '0';\\n                                        sentIndicator.style.transition = 'opacity 0.3s ease';\\n                                        \\n                                        clickedElement.style.position = 'relative';\\n                                        clickedElement.appendChild(sentIndicator);\\n                                        \\n                                        // æ˜¾ç¤ºå¹¶æ·¡å‡ºæŒ‡ç¤ºå™¨\\n                                        setTimeout(() => {\\n                                            sentIndicator.style.opacity = '1';\\n                                            setTimeout(() => {\\n                                                sentIndicator.style.opacity = '0';\\n                                                setTimeout(() => {\\n                                                    sentIndicator.remove();\\n                                                }, 300);\\n                                            }, 1500);\\n                                        }, 10);\\n                                        \\n                                    } else {\\n                                        console.error('æœªæ‰¾åˆ°SillyTavernçš„å‘é€æŒ‰é’®');\\n                                        fallbackCopyTextToClipboard(message);\\n                                    }\\n                                } else {\\n                                    console.error('æœªæ‰¾åˆ°SillyTavernçš„è¾“å…¥æ¡†');\\n                                    fallbackCopyTextToClipboard(message);\\n                                }\\n                            } catch (error) {\\n                                console.error('æ— æ³•ç›´æ¥ä¸SillyTavernäº¤äº’:', error);\\n                                fallbackCopyTextToClipboard(message);\\n                            }\\n                        }\\n                        \\n                        // å›é€€æ–¹æ¡ˆï¼šå¤åˆ¶åˆ°å‰ªè´´æ¿\\n                        function fallbackCopyTextToClipboard(text) {\\n                            const copyTextArea = document.createElement('textarea');\\n                            copyTextArea.value = text;\\n                            copyTextArea.style.position = 'fixed';\\n                            copyTextArea.style.opacity = '0';\\n                            document.body.appendChild(copyTextArea);\\n                            copyTextArea.select();\\n                            \\n                            try {\\n                                const successful = document.execCommand('copy');\\n                                const msg = successful ? 'æ¶ˆæ¯å·²å¤åˆ¶åˆ°å‰ªè´´æ¿ï¼Œè¯·ç²˜è´´åˆ°SillyTavern' : 'å¤åˆ¶å¤±è´¥';\\n                                alert(msg);\\n                            } catch (err) {\\n                                console.error('æ— æ³•å¤åˆ¶åˆ°å‰ªè´´æ¿:', err);\\n                                alert('æ— æ³•è‡ªåŠ¨å‘é€æ¶ˆæ¯åˆ°SillyTavern');\\n                            }\\n                            \\n                            document.body.removeChild(copyTextArea);\\n                        }\\n                        \\n                        // åˆå§‹è°ƒç”¨ä¸€æ¬¡\\n                        addClickToSendForChoices();\\n                        \\n                        // å¦‚æœä½ çš„é€‰é¡¹å¡æ˜¯åŠ¨æ€åŠ è½½çš„ï¼Œä½ å¯èƒ½éœ€è¦åœ¨åŠ è½½æ–°å†…å®¹åå†æ¬¡è°ƒç”¨æ­¤å‡½æ•°\\n                    });\\n                        // é¡µé¢åˆ‡æ¢åŠŸèƒ½\\n                        document.addEventListener('DOMContentLoaded', function() {\\n                                // åˆ›å»ºå…¨å±é¢„è§ˆçš„å®¹å™¨\\n                        const overlay = document.createElement('div');\\n                        overlay.className = 'fullscreen-image-overlay';\\n                        \\n                        // åˆ›å»ºå›¾ç‰‡å…ƒç´ \\n                        const fullImage = document.createElement('img');\\n                        fullImage.className = 'fullscreen-image';\\n                        \\n                        // åˆ›å»ºå…³é—­æŒ‰é’®\\n                        const closeButton = document.createElement('div');\\n                        closeButton.className = 'close-fullscreen';\\n                        closeButton.innerHTML = 'Ã—';\\n                        \\n                        // ç»„è£…å…ƒç´ \\n                        overlay.appendChild(fullImage);\\n                        overlay.appendChild(closeButton);\\n                        document.body.appendChild(overlay);\\n                        \\n                        // ä¸ºæ‰€æœ‰banneræ·»åŠ ç‚¹å‡»äº‹ä»¶\\n                        const banners = document.querySelectorAll('.character-banner');\\n                        banners.forEach(banner => {\\n                            banner.addEventListener('click', function() {\\n                                // è·å–bannerä¸­çš„å›¾ç‰‡URL\\n                                let imgSrc = '';\\n                                const bannerImg = banner.querySelector('img');\\n                                \\n                                if (bannerImg) {\\n                                    // å¦‚æœbannerä¸­æœ‰å›¾ç‰‡å…ƒç´ ï¼Œä½¿ç”¨å…¶src\\n                                    imgSrc = bannerImg.src;\\n                                } else {\\n                                    // å¦‚æœæ²¡æœ‰å›¾ç‰‡å…ƒç´ ï¼Œåˆ™å°è¯•è·å–èƒŒæ™¯å›¾ç‰‡URL\\n                                    const style = window.getComputedStyle(banner);\\n                                    const bgImage = style.backgroundImage;\\n                                    \\n                                    if (bgImage && bgImage !== 'none') {\\n                                        imgSrc = bgImage.replace(/^url\\\\(['\\\"](.+)['\\\"]\\\\)$/, '$1');\\n                                    }\\n                                }\\n                                \\n                                // å¦‚æœæˆåŠŸè·å–åˆ°å›¾ç‰‡ï¼Œæ˜¾ç¤ºå…¨å±é¢„è§ˆ\\n                                if (imgSrc) {\\n                                    fullImage.src = imgSrc;\\n                                    overlay.classList.add('active');\\n                                }\\n                            });\\n                        });\\n                        const sceneImages = document.querySelectorAll('.scene-image');\\n                            sceneImages.forEach(image => {\\n                                image.addEventListener('click', function() {\\n                                    fullImage.src = image.src;\\n                                    overlay.classList.add('active');\\n                                });\\n                            });\\n\\n                        // ç‚¹å‡»å…³é—­æŒ‰é’®æˆ–èƒŒæ™¯å…³é—­é¢„è§ˆ\\n                        closeButton.addEventListener('click', function() {\\n                            overlay.classList.remove('active');\\n                        });\\n                        \\n                        overlay.addEventListener('click', function(e) {\\n                            if (e.target === overlay) {\\n                                overlay.classList.remove('active');\\n                            }\\n                        });\\n                            // æ ‡ç­¾åˆ‡æ¢\\n                            const navItems = document.querySelectorAll('.nav-item');\\n                            navItems.forEach(item => {\\n                                item.addEventListener('click', function() {\\n                                    // ç§»é™¤æ‰€æœ‰æ´»è·ƒçŠ¶æ€\\n                                    navItems.forEach(nav => nav.classList.remove('active'));\\n                                    this.classList.add('active');\\n                                    \\n                                    // åˆ‡æ¢é¡µé¢\\n                                    const target = this.getAttribute('data-target');\\n                                    document.querySelectorAll('.page').forEach(page => {\\n                                        page.classList.remove('active');\\n                                    });\\n                                    document.getElementById(target).classList.add('active');\\n                                });\\n                            });\\n                            const navIcons = document.querySelectorAll('.nav-icon');\\n                    navIcons.forEach(icon => {\\n                        icon.style.display = 'block';\\n                        icon.style.visibility = 'visible';\\n                        icon.style.opacity = '1';\\n                    });\\n                            // ç¡®ä¿æ—¶é—´æŒ‰é’®å’Œé¢æ¿æ­£å¸¸å·¥ä½œ\\n                const timeBtn = document.querySelector('.time-btn');\\n                const timePanel = document.querySelector('.time-panel');\\n                \\n                if (timeBtn && timePanel) {\\n                    timeBtn.addEventListener('mouseenter', function() {\\n                        timePanel.style.transform = 'translateX(0)';\\n                        timePanel.style.opacity = '1';\\n                        timePanel.style.visibility = 'visible';\\n                    });\\n                    \\n                    timeBtn.addEventListener('mouseleave', function(e) {\\n                        // æ£€æŸ¥é¼ æ ‡æ˜¯å¦ç§»åŠ¨åˆ°äº†æ—¶é—´é¢æ¿ä¸Š\\n                        const rect = timePanel.getBoundingClientRect();\\n                        if (\\n                            e.clientX < rect.left ||\\n                            e.clientX > rect.right ||\\n                            e.clientY < rect.top ||\\n                            e.clientY > rect.bottom\\n                        ) {\\n                            timePanel.style.transform = 'translateX(20px)';\\n                            timePanel.style.opacity = '0';\\n                            timePanel.style.visibility = 'hidden';\\n                        }\\n                    });\\n                    \\n                    timePanel.addEventListener('mouseleave', function() {\\n                        timePanel.style.transform = 'translateX(20px)';\\n                        timePanel.style.opacity = '0';\\n                        timePanel.style.visibility = 'hidden';\\n                    });\\n                }\\n                            // é¢œè‰²é€‰æ‹©\\n                            const colorOptions = document.querySelectorAll('.color-option');\\n                                colorOptions.forEach(option => {\\n                                    option.addEventListener('click', function() {\\n                                        colorOptions.forEach(opt => opt.classList.remove('active'));\\n                                        this.classList.add('active');\\n                                        \\n                                        // è·å–é¢œè‰²å¹¶åº”ç”¨åˆ°å­—ä½“\\n                                        const color = this.getAttribute('data-color');\\n                                        document.querySelector('.scene-text').style.color = color;\\n                                    });\\n                                });\\n                \\n                                            \\n                            // å­—ä½“å¤§å°è°ƒæ•´\\n                            const fontSizeRange = document.getElementById('font-size-range');\\n                            const fontSizeValue = document.getElementById('font-size-value');\\n\\n                            \\n                            fontSizeRange.addEventListener('input', function() {\\n                                fontSizeValue.textContent = this.value + 'px';\\n                                document.querySelector('.scene-text').style.fontSize = this.value + 'px';\\n                            });\\n                            \\n                            // é€æ˜åº¦è°ƒæ•´\\n                            const opacityRange = document.getElementById('notebook-opacity');\\n                            const opacityValue = document.getElementById('opacity-value');\\n                            \\n                            opacityRange.addEventListener('input', function() {\\n                                const value = Math.round(this.value * 100) + '%';\\n                                opacityValue.textContent = value;\\n                                document.querySelector('.app-container').style.opacity = this.value;\\n                            });\\n                            \\n                            // å­—ä½“é€‰æ‹©\\n                            const fontFamilySelect = document.getElementById('font-family-select');\\n                            fontFamilySelect.addEventListener('change', function() {\\n                                let fontFamily = '';\\n                                switch(this.value) {\\n                                    case 'mashanzheng':\\n                                        fontFamily = \\\"'Ma Shan Zheng', cursive\\\";\\n                                        break;\\n                                    case 'zhimangxing':\\n                                        fontFamily = \\\"'Zhi Mang Xing', cursive\\\";\\n                                        break;\\n                                    case 'liujianmaocao':\\n                                        fontFamily = \\\"'Liu Jian Mao Cao', cursive\\\";\\n                                        break;\\n                                    case 'longcang':\\n                                        fontFamily = \\\"'Long Cang', cursive\\\";\\n                                        break;\\n                                    case 'kai':\\n                                        fontFamily = \\\"'æ¥·ä½“', 'STKaiti', KaiTi, sans-serif\\\";\\n                                        break;\\n                                    case 'song':\\n                                        fontFamily = \\\"'å®‹ä½“', 'SimSun', serif\\\";\\n                                        break;\\n                                    case 'hei':\\n                                        fontFamily = \\\"'é»‘ä½“', 'SimHei', sans-serif\\\";\\n                                        break;\\n                                    case 'fangsong':\\n                                        fontFamily = \\\"'ä»¿å®‹', 'STFangsong', FangSong, serif\\\";\\n                                        break;\\n                                    default:\\n                                        fontFamily = \\\"'å¾®è½¯é›…é»‘', 'Arial', sans-serif\\\";\\n                                }\\n                                document.querySelector('.scene-text').style.fontFamily = fontFamily;\\n                            });\\n                            \\n                            // ç¬”è®°æœ¬å¤§å°è®¾ç½®\\n                            const notebookSizeSelect = document.getElementById('notebook-size-select');\\n                            notebookSizeSelect.addEventListener('change', function() {\\n                                const bookContainer = document.querySelector('.book-container');\\n                                switch(this.value) {\\n                                    case 'large':\\n                                        bookContainer.style.width = '100%';\\n                                        break;\\n                                    case 'small':\\n                                        bookContainer.style.width = '80%';\\n                                        break;\\n                                    case 'medium':\\n                                        bookContainer.style.width = '90%';\\n                                        break;\\n                                    default:\\n                                        bookContainer.style.width = '95%';\\n                                }\\n                            });\\n                            \\n                            \\n                            // è¿˜åŸé»˜è®¤è®¾ç½®\\n                            document.getElementById('reset-settings').addEventListener('click', function() {\\n                                if (confirm('ç¡®å®šè¦æ¢å¤é»˜è®¤è®¾ç½®å—ï¼Ÿ')) {\\n                                    // æ¢å¤å­—ä½“å¤§å°\\n                                    fontSizeRange.value = 18;\\n                                    fontSizeValue.textContent = '18px';\\n                                    document.querySelector('.scene-text').style.fontSize = '18px';\\n                                    \\n                                    // æ¢å¤é€æ˜åº¦\\n                                    opacityRange.value = 1;\\n                                    opacityValue.textContent = '100%';\\n                                    document.querySelector('.app-container').style.opacity = 1;\\n                                    \\n                                    // æ¢å¤å­—ä½“\\n                                    fontFamilySelect.value = 'default';\\n                                    document.querySelector('.scene-text').style.fontFamily = \\\"'å¾®è½¯é›…é»‘', 'Arial', sans-serif\\\";\\n                                    \\n                                    // æ¢å¤ç¬”è®°æœ¬å¤§å°\\n                                    notebookSizeSelect.value = 'medium';\\n                                    document.querySelector('.book-container').style.width = '90%';\\n                                    \\n                                    // æ¢å¤é¢œè‰²\\n                                    colorOptions.forEach(opt => opt.classList.remove('active'));\\n                                    colorOptions[0].classList.add('active');\\n                                    document.querySelector('.scene-container').style.background = 'rgba(255, 255, 255, 0.7)';\\n                                    \\n                                    alert('å·²æ¢å¤é»˜è®¤è®¾ç½®');\\n                                }\\n                            });\\n                        });\\n                        // åŠ è½½åŠ¨ç”»æ§åˆ¶\\n                        <!-- ä¿®æ”¹åŠ è½½åŠ¨ç”»åˆå§‹åŒ–å‡½æ•° -->\\n                function initLoadingAnimation() {\\n                    // ç¡®ä¿DOMå·²åŠ è½½\\n                    if (document.readyState === 'loading') {\\n                        document.addEventListener('DOMContentLoaded', initLoadingAnimation);\\n                        return;\\n                    }\\n                    \\n                    // åˆ›å»ºèƒŒæ™¯ç²’å­å’ŒèŠ±ç“£\\n                    createParticles();\\n                    createPetals();\\n                    \\n                    // åˆå§‹åŒ–æç¤ºè½®æ¢\\n                    currentTip = 0; // ç¡®ä¿ä»ç¬¬ä¸€æ¡æç¤ºå¼€å§‹\\n                    rotateTips();\\n                    \\n                    // æ›´æ–°åŠ è½½æ–‡æœ¬\\n                    updateLoadingText();\\n                    \\n                    // è®¾ç½®è§’è‰²åˆ‡æ¢ç‚¹\\n                    setupCharacterDots();\\n                    \\n                    // å‡ ç§’åéšè—åŠ è½½åŠ¨ç”»ï¼Œæ˜¾ç¤ºä¸»ç•Œé¢\\n                    setTimeout(() => {\\n                        const loadingAnimation = document.getElementById('loading-animation');\\n                        const bookContainer = document.querySelector('.book-container');\\n                        const avatarImg = document.getElementById('character-avatar');\\n                        const target = document.querySelector('.logo');\\n\\n                        // éšè—åŠ è½½åŠ¨ç”»\\n                        loadingAnimation.style.opacity = '0';\\n                        loadingAnimation.style.visibility = 'hidden';\\n\\n                        // ç­‰åŠ è½½åŠ¨ç”»å®Œå…¨éšè—åå†æ˜¾ç¤ºä¸»ç•Œé¢å¹¶æ‰§è¡Œé£è¡ŒåŠ¨ç”»\\n                        setTimeout(() => {\\n                            bookContainer.style.display = 'block';\\n\\n                            // ç­‰ä¸»ç•Œé¢æ¸²æŸ“å‡ºæ¥\\n                            setTimeout(() => {\\n                                // å…‹éš†å¤´åƒå¹¶åŠ å…¥é¡µé¢\\n                                const flyImg = avatarImg.cloneNode(true);\\n                                flyImg.className = 'fly-avatar';\\n                                document.body.appendChild(flyImg);\\n\\n                                // è·å–åŸå§‹ä½ç½®ï¼ˆä¸­å¿ƒï¼‰å’Œç›®æ ‡ä½ç½®ï¼ˆlogoï¼‰\\n                                const startRect = avatarImg.getBoundingClientRect();\\n                                const endRect = target.getBoundingClientRect();\\n\\n                                flyImg.style.left = `${startRect.left}px`;\\n                                flyImg.style.top = `${startRect.top}px`;\\n\\n                                // è§¦å‘åŠ¨ç”»\\n                                flyImg.getBoundingClientRect(); // å¼ºåˆ¶é‡ç»˜\\n                                const dx = endRect.left - startRect.left;\\n                                const dy = endRect.top - startRect.top;\\n                                flyImg.style.transform = `translate(${dx}px, ${dy}px) scale(0.5)`;\\n                                flyImg.style.opacity = '0';\\n\\n                                // ç§»é™¤é£è¡ŒåŠ¨ç”»å›¾ç‰‡\\n                                setTimeout(() => {\\n                                    flyImg.remove();\\n                                }, 1000);\\n                            }, 100); // ç­‰å¾…ä¸€ç‚¹æ—¶é—´è®©ä¸»é¡µé¢ DOM æ¸²æŸ“\\n                        }, 1000); // ç­‰åŠ è½½åŠ¨ç”»å®Œå…¨éšè—åå†æ‰§è¡Œ\\n                    }, 5500); // åŸå§‹åŠ è½½æ—¶é•¿\\n                }\\n                        // åˆ›å»ºèƒŒæ™¯ç²’å­\\n                function createParticles() {\\n                    const container = document.getElementById('particles');\\n                    const particlesCount = 20; // å‡å°‘ç²’å­æ•°é‡ä»¥æé«˜æ€§èƒ½\\n                    \\n                    for (let i = 0; i < particlesCount; i++) {\\n                        const particle = document.createElement('div');\\n                        particle.className = 'particle';\\n                        \\n                        // éšæœºå¤§å°ï¼Œä½†è¾ƒå°ä»¥å‡è½»æ¸²æŸ“è´Ÿæ‹…\\n                        const size = Math.random() * 20 + 5;\\n                        particle.style.width = `${size}px`;\\n                        particle.style.height = `${size}px`;\\n                        \\n                        // éšæœºä½ç½®\\n                        particle.style.left = `${Math.random() * 100}%`;\\n                        \\n                        // éšæœºåŠ¨ç”»æ—¶é—´\\n                        const duration = Math.random() * 10 + 10;\\n                        particle.style.animationDuration = `${duration}s`;\\n                        \\n                        // éšæœºå»¶è¿Ÿ\\n                        const delay = Math.random() * 5;\\n                        particle.style.animationDelay = `${delay}s`;\\n                        \\n                        // éšæœºæ·±åº¦\\n                        const zIndex = Math.random() * 300 - 150;\\n                        particle.style.transform = `translateZ(${zIndex}px)`;\\n                        \\n                        container.appendChild(particle);\\n                    }\\n                }\\n                \\n                // åˆ›å»ºèŠ±ç“£\\n                function createPetals() {\\n                    const container = document.getElementById('flower-petals');\\n                    const petalsCount = 8;\\n                    \\n                    for (let i = 0; i < petalsCount; i++) {\\n                        const petal = document.createElement('div');\\n                        petal.className = 'petal';\\n                        \\n                        // è®¡ç®—ä½ç½®ï¼ˆå›´ç»•ä¸­å¿ƒå‡åŒ€åˆ†å¸ƒï¼‰\\n                        const angle = (i / petalsCount) * Math.PI * 2;\\n                        const distance = 50; // è·ç¦»ä¸­å¿ƒçš„è·ç¦»ç™¾åˆ†æ¯”\\n                        \\n                        const x = Math.cos(angle) * distance;\\n                        const y = Math.sin(angle) * distance;\\n                        \\n                        // ä½¿ç”¨ç™¾åˆ†æ¯”å®šä½ä»¥é€‚åº”ä¸åŒå°ºå¯¸\\n                        petal.style.left = `${x + 50 - 11}%`; // 50%æ˜¯containerå®½åº¦çš„ä¸€åŠï¼Œ11%æ˜¯petalå®½åº¦çš„ä¸€åŠ\\n                        petal.style.top = `${y + 50 - 11}%`;\\n                        \\n                        // è®¾ç½®æ—‹è½¬è§’åº¦ï¼Œä½¿èŠ±ç“£æŒ‡å‘ä¸­å¿ƒ\\n                        const rotateDeg = (angle * 180 / Math.PI) + 90;\\n                        petal.style.transform = `rotate(${rotateDeg}deg) translateZ(30px)`;\\n                        \\n                        container.appendChild(petal);\\n                    }\\n                }\\n                \\n                // è½®æ¢åŠ è½½æç¤º\\n                const tips = [\\n                    \\\"æ¸¸ç©ä¹‹å‰å¯ä»¥é˜…è¯»æ•´ä¸ªä¸–ç•Œè®¾å®šï¼Œä¹Ÿå¯ä»¥å½“åšç©¿è¶Šè€…ã€‚\\\", // ä½¿ç”¨è‹±æ–‡é€—å·ï¼Œä¸æ˜¯ä¸­æ–‡é€—å·\\n                    \\\"åœ¨æ­£å¼è¿›å»å¥½æ„Ÿåº¦ä¹‹å‰ä¼šç»å†åŠå°æ—¶çš„è€ƒå¯ŸæœŸï¼Œæ­¤æœŸé—´å¯¹è§’è‰²çš„è¡¨ç°ä¼šç›´æ¥ååº”åˆ°æ­£å¼çš„å¥½æ„Ÿåº¦ã€‚\\\",\\n                    \\\"è¿èƒŒè§’è‰²æ„æ„¿ä¼šé™ä½å¥½æ„Ÿåº¦ï¼Œå¼ºè¡Œå‘ç”Ÿå…³ç³»å¯èƒ½ä¼šå¯¼è‡´è¯¥è§’è‰²å¥½æ„Ÿåº¦é”æ­»ã€‚\\\",\\n                ];\\n                \\n                let currentTip = 0;\\n                \\n                function rotateTips() {\\n                    const tipContent = document.getElementById('tip-content');\\n                    \\n                    // å…ˆè®¾ç½®åˆå§‹æç¤º\\n                    tipContent.textContent = tips[currentTip];\\n                    \\n                    setInterval(() => {\\n                        // å…ˆæ·¡å‡ºå½“å‰æç¤º\\n                        tipContent.style.opacity = 0;\\n                        \\n                        setTimeout(() => {\\n                            // åˆ‡æ¢åˆ°ä¸‹ä¸€ä¸ªæç¤º\\n                            currentTip = (currentTip + 1) % tips.length;\\n                            tipContent.textContent = tips[currentTip];\\n                            // è®©æ–°æç¤ºæ·¡å…¥\\n                            tipContent.style.opacity = 1;\\n                        }, 500);\\n                    }, 2000);\\n                }\\n                \\n                // æ›´æ–°åŠ è½½æ–‡æœ¬\\n                function updateLoadingText() {\\n                    const loadingTexts = [\\n                        \\\"è½½å…¥ä¸­ - å‡†å¤‡æ ¡å›­èµ„æº\\\",\\n                        \\\"è½½å…¥ä¸­ - åŠ è½½è§’è‰²æ•°æ®\\\",\\n                        \\\"è½½å…¥ä¸­ - æ„å»ºåœºæ™¯ä¿¡æ¯\\\"\\n                    ];\\n                    \\n                    const loadingTextElement = document.getElementById('loading-text');\\n                    let currentText = 0;\\n                    \\n                    setInterval(() => {\\n                        currentText = (currentText + 1) % loadingTexts.length;\\n                        loadingTextElement.textContent = loadingTexts[currentText];\\n                    }, 1200);\\n                }\\n                \\n                // è®¾ç½®è§’è‰²åˆ‡æ¢ç‚¹çš„ç‚¹å‡»äº‹ä»¶\\n                function setupCharacterDots() {\\n                    // æ·»åŠ è§’è‰²é€‰æ‹©ç‚¹\\n                    const indicatorsContainer = document.querySelector('.character-indicators');\\n                    const characters = [\\n                        { name: \\\"æ—èŠ·è‹¥\\\", image: \\\"https://files.catbox.moe/6iejyf.png\\\" }, \\n                        { name: \\\"èµµæ¢¦çª\\\", image: \\\"https://files.catbox.moe/linzhiruo.png\\\" },\\n                        { name: \\\"è‹é›¨æ™´\\\", image: \\\"https://files.catbox.moe/suyuqing.png\\\" }\\n                    ];\\n                    \\n                    // åˆ›å»ºæŒ‡ç¤ºç‚¹\\n                    characters.forEach((character, index) => {\\n                        const dot = document.createElement('div');\\n                        dot.className = 'character-dot';\\n                        dot.setAttribute('data-name', character.name);\\n                        dot.setAttribute('data-image', character.image);\\n                        indicatorsContainer.appendChild(dot);\\n                    });\\n                    \\n                    // æ·»åŠ ç‚¹å‡»äº‹ä»¶\\n                    const dots = document.querySelectorAll('.character-dot');\\n                    const avatarImg = document.getElementById('character-avatar');\\n                    const nameElement = document.getElementById('character-name');\\n                    \\n                    dots.forEach(dot => {\\n                        dot.addEventListener('click', function() {\\n                            // ç§»é™¤æ‰€æœ‰æ¿€æ´»çŠ¶æ€\\n                            dots.forEach(d => d.classList.remove('active'));\\n                            \\n                            // æ·»åŠ å½“å‰ç‚¹çš„æ¿€æ´»çŠ¶æ€\\n                            this.classList.add('active');\\n                            \\n                            // æ›´æ–°å¤´åƒå’Œåç§°\\n                            const name = this.getAttribute('data-name');\\n                            const image = this.getAttribute('data-image');\\n                            \\n                            // æ·»åŠ æ·¡å‡ºåŠ¨ç”»\\n                            avatarImg.style.opacity = 0;\\n                            \\n                            // å»¶è¿Ÿåæ›´æ¢å›¾ç‰‡å¹¶æ·¡å…¥\\n                            setTimeout(() => {\\n                                avatarImg.src = image;\\n                                nameElement.textContent = name;\\n                                \\n                                avatarImg.style.opacity = 1;\\n                            }, 300);\\n                        });\\n                    });\\n                }\\n                \\n                // ç«‹å³æ‰§è¡Œåˆå§‹åŒ–åŠ è½½åŠ¨ç”»\\n                initLoadingAnimation();\\n\\n// è®¾ç½®ç®¡ç†å™¨\\nconst SettingsManager = {\\n    // ä¿å­˜æ‰€æœ‰è®¾ç½®åˆ°æœ¬åœ°å­˜å‚¨\\n    saveSettings() {\\n        const settings = {\\n            fontSize: document.getElementById('font-size-range').value,\\n            opacity: document.getElementById('notebook-opacity').value,\\n            fontFamily: document.getElementById('font-family-select').value,\\n            notebookSize: document.getElementById('notebook-size-select').value,\\n            textColor: document.querySelector('.color-option.active')?.getAttribute('data-color') || '#333333',\\n            // æ–°å¢è®¾ç½®é¡¹\\n            contentOpacity: document.getElementById('content-opacity-range').value,\\n            contentBgColor: document.querySelector('#content-bg-colors .bgcolor-option.active')?.getAttribute('data-color') || '#ffffff',\\n            outerBgColor: document.querySelector('#outer-bg-colors .outbgcolor-option.active')?.getAttribute('data-color') || 'transparent'\\n        };\\n\\n        localStorage.setItem('notebookSettings', JSON.stringify(settings));\\n        console.log('è®¾ç½®å·²ä¿å­˜:', settings);\\n    },\\n\\n    // ä»æœ¬åœ°å­˜å‚¨åŠ è½½è®¾ç½®\\n    loadSettings() {\\n        const savedSettingsStr = localStorage.getItem('notebookSettings');\\n        if (!savedSettingsStr) return false;\\n\\n        try {\\n            const settings = JSON.parse(savedSettingsStr);\\n            console.log('æ­£åœ¨åŠ è½½è®¾ç½®:', settings);\\n            \\n            // åº”ç”¨å­—ä½“å¤§å°\\n            const fontSizeRange = document.getElementById('font-size-range');\\n            const fontSizeValue = document.getElementById('font-size-value');\\n            if (fontSizeRange && fontSizeValue && settings.fontSize) {\\n                fontSizeRange.value = settings.fontSize;\\n                fontSizeValue.textContent = settings.fontSize + 'px';\\n                document.querySelector('.scene-text').style.fontSize = settings.fontSize + 'px';\\n            }\\n            \\n            // åº”ç”¨é€æ˜åº¦\\n            const opacityRange = document.getElementById('notebook-opacity');\\n            const opacityValue = document.getElementById('opacity-value');\\n            if (opacityRange && opacityValue && settings.opacity) {\\n                opacityRange.value = settings.opacity;\\n                opacityValue.textContent = Math.round(settings.opacity * 100) + '%';\\n                document.querySelector('.app-container').style.opacity = settings.opacity;\\n            }\\n            \\n            // åº”ç”¨å­—ä½“\\n            const fontSelect = document.getElementById('font-family-select');\\n            if (fontSelect && settings.fontFamily) {\\n                fontSelect.value = settings.fontFamily;\\n                this.applyFontFamily(settings.fontFamily);\\n            }\\n            \\n            // åº”ç”¨ç¬”è®°æœ¬å¤§å°\\n            const sizeSelect = document.getElementById('notebook-size-select');\\n            if (sizeSelect && settings.notebookSize) {\\n                sizeSelect.value = settings.notebookSize;\\n                this.applyNotebookSize(settings.notebookSize);\\n            }\\n            \\n            // åº”ç”¨æ–‡æœ¬é¢œè‰²\\n            if (settings.textColor) {\\n                const colorOptions = document.querySelectorAll('.color-option');\\n                colorOptions.forEach(option => {\\n                    if (option.parentNode.id !== 'content-bg-colors' && \\n                        option.parentNode.id !== 'outer-bg-colors') {\\n                        option.classList.remove('active');\\n                        if (option.getAttribute('data-color') === settings.textColor) {\\n                            option.classList.add('active');\\n                            document.querySelector('.scene-text').style.color = settings.textColor;\\n                        }\\n                    }\\n                });\\n            }\\n            \\n            // åº”ç”¨æ­£æ–‡èƒŒæ™¯é€æ˜åº¦\\n            const contentOpacityRange = document.getElementById('content-opacity-range');\\n            const contentOpacityValue = document.getElementById('content-opacity-value');\\n            if (contentOpacityRange && contentOpacityValue && settings.contentOpacity) {\\n                contentOpacityRange.value = settings.contentOpacity;\\n                contentOpacityValue.textContent = Math.round(settings.contentOpacity * 100) + '%';\\n                this.applyContentOpacity(settings.contentOpacity);\\n            }\\n            \\n            // åº”ç”¨æ­£æ–‡èƒŒæ™¯é¢œè‰²\\n            if (settings.contentBgColor) {\\n                const contentBgOptions = document.querySelectorAll('#content-bg-colors .bgcolor-option');\\n                contentBgOptions.forEach(option => {\\n                    option.classList.remove('active');\\n                    if (option.getAttribute('data-color') === settings.contentBgColor) {\\n                        option.classList.add('active');\\n                        this.applyContentBgColor(settings.contentBgColor);\\n                    }\\n                });\\n            }\\n            \\n            // åº”ç”¨å¤–å±‚èƒŒæ™¯é¢œè‰²\\n            if (settings.outerBgColor) {\\n                const outerBgOptions = document.querySelectorAll('#outer-bg-colors .outbgcolor-option');\\n                outerBgOptions.forEach(option => {\\n                    option.classList.remove('active');\\n                    if (option.getAttribute('data-color') === settings.outerBgColor) {\\n                        option.classList.add('active');\\n                        this.applyOuterBgColor(settings.outerBgColor);\\n                    }\\n                });\\n            }\\n            \\n            return true;\\n        } catch (e) {\\n            console.error('åŠ è½½è®¾ç½®å¤±è´¥:', e);\\n            return false;\\n        }\\n    },\\n\\n    // åº”ç”¨å­—ä½“\\n    applyFontFamily(value) {\\n        let fontFamily = '';\\n        switch(value) {\\n            case 'mashanzheng':\\n                fontFamily = \\\"'Ma Shan Zheng', cursive\\\";\\n                break;\\n            case 'zhimangxing':\\n                fontFamily = \\\"'Zhi Mang Xing', cursive\\\";\\n                break;\\n            case 'liujianmaocao':\\n                fontFamily = \\\"'Liu Jian Mao Cao', cursive\\\";\\n                break;\\n            case 'longcang':\\n                fontFamily = \\\"'Long Cang', cursive\\\";\\n                break;\\n            case 'kai':\\n                fontFamily = \\\"'æ¥·ä½“', 'STKaiti', KaiTi, sans-serif\\\";\\n                break;\\n            case 'song':\\n                fontFamily = \\\"'å®‹ä½“', 'SimSun', serif\\\";\\n                break;\\n            case 'hei':\\n                fontFamily = \\\"'é»‘ä½“', 'SimHei', sans-serif\\\";\\n                break;\\n            case 'fangsong':\\n                fontFamily = \\\"'ä»¿å®‹', 'STFangsong', FangSong, serif\\\";\\n                break;\\n            default:\\n                fontFamily = \\\"'å¾®è½¯é›…é»‘', 'Microsoft YaHei', 'Arial', sans-serif\\\";\\n        }\\n        document.querySelector('.scene-text').style.fontFamily = fontFamily;\\n\\n        // åŒæ—¶åº”ç”¨åˆ°æ‰€æœ‰æ–‡æœ¬å†…å®¹ä»¥ä¿æŒä¸€è‡´æ€§\\n        document.querySelectorAll('.scene-container, .character-details').forEach(el => {\\n            el.style.fontFamily = fontFamily;\\n        });\\n    },\\n\\n    // åº”ç”¨ç¬”è®°æœ¬å¤§å°\\n    applyNotebookSize(value) {\\n        const bookContainer = document.querySelector('.book-container');\\n        switch(value) {\\n            case 'large':\\n                bookContainer.style.width = '95%';\\n                break;\\n            case 'small':\\n                bookContainer.style.width = '80%';\\n                break;\\n            default:\\n                bookContainer.style.width = '90%';\\n        }\\n    },\\n    \\n    // åº”ç”¨æ­£æ–‡èƒŒæ™