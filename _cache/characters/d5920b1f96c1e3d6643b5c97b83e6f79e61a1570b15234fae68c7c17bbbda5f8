ä¸­çš„æ‰€æœ‰é€‰é¡¹å¡ç‰‡\\n                            const choiceCards = document.querySelectorAll('.style-modern .choice-card');\\n                            \\n                            choiceCards.forEach(card => {\\n                                card.addEventListener('click', function() {\\n                                    // è·å–é€‰é¡¹å†…å®¹å’Œæ ‡é¢˜\\n                                    const choiceContent = this.querySelector('.choice-content').textContent.trim();\\n                                    const choiceHeader = this.querySelector('.choice-header').textContent.trim();\\n                                    \\n                                    // æ ¼å¼åŒ–è¦å‘é€çš„æ¶ˆæ¯ï¼ˆå¯ä»¥æ ¹æ®éœ€è¦è°ƒæ•´æ ¼å¼ï¼‰\\n                                    const msg = `{{user}}é€‰æ‹©äº†: ${choiceContent}`;\\n                                    \\n                                    // å°è¯•ç›´æ¥å‘é€åˆ°SillyTavern\\n                                    sendToSillyTavern(msg, this);\\n                                    \\n                                    // å¯é€‰ï¼šæ·»åŠ é€‰æ‹©åçš„è§†è§‰åé¦ˆ\\n                                    choiceCards.forEach(c => {\\n                                        c.style.opacity = '0.5';\\n                                        c.style.transform = 'none';\\n                                    });\\n                                    this.style.opacity = '1';\\n                                    this.style.borderColor = '#f8d56f';\\n                                    this.style.transform = 'translateY(-5px)';\\n                                });\\n                            });\\n                        };\\n                        \\n                        // å‘é€æ¶ˆæ¯åˆ°SillyTavernçš„å‡½æ•°\\n                        function sendToSillyTavern(message, clickedElement) {\\n                            try {\\n                                // å°è¯•è®¿é—®çˆ¶çª—å£(SillyTavern)çš„DOM\\n                                cons