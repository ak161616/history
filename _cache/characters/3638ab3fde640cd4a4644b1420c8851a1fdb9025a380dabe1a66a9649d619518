Padding = 20; // Total 10px top + 10px bottom\\n                const bodyHorizontalPadding = 0; \\n\\n                // Use clientWidth/Height of documentElement for a more robust measure of viewport\\n                const viewportWidth = document.documentElement.clientWidth;\\n                const viewportHeight = document.documentElement.clientHeight;\\n\\n                const availableWidth = viewportWidth - bodyHorizontalPadding;\\n                const availableHeight = viewportHeight - bodyVerticalPadding;\\n\\n                let scale = 1; // Default to no scaling if dimensions are problematic\\n\\n                if (phoneWidth > 0 && phoneHeight > 0 && availableWidth > 0 && availableHeight > 0) {\\n                    const scaleX = availableWidth / phoneWidth;\\n                    const scaleY = availableHeight / phoneHeight;\\n                    scale = Math.min(1, scaleX, scaleY); // Do not scale up, only down if needed\\n                }\\n                \\n                // Ensure scale is not excessively small, e.g., due to tiny iframe or miscalculation\\n                // scale = Math.max(0.4, scale); // Temporarily disabling dynamic scale for diagnosis\\n\\n                phoneOuterShell.style.transform = 'scale(1)'; // DIAGNOSTIC: Force scale to 1\\n                // transform-origin is 'center center' in CSS\\n            }\\n            document.body.style.minHeight = '100vh'; // Restored: Ensure body has height for expanded view\\n            document.body.style.padding = '10px 0'; // Padding for expanded view\\n            isExpanded = true;\\n        }\\n\\n        function collapseView() {\\n            if (phoneOuterShell) {\\n                phoneOuterShell.style.display = 'none';\\n                phoneOuterShell.style.transform = 'scale(1)'; // Reset scale\\n            }\\n            if (momoToggler) momoToggler.style.display = 'flex';\\n            document.body.style.minHeight = 'auto'; // Restored: Body height for colla