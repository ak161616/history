if (!profile) {\\n                // Clear fields if no profile\\n                profileAvatarImg.src = '';\\n                profileAvatarImg.alt = '头像';\\n                profileNameDisplay.textContent = '没有人物数据';\\n                profileTitleDisplay.textContent = '';\\n                profileInfoDisplay.textContent = '';\\n                currentProfileBio = '';\\n                currentProfileStory = '';\\n                // if (viewDetailsButton) viewDetailsButton.style.display = 'none'; // Button is removed\\n                isShowingStory = false; // Reset state\\n                return;\\n            }\\n\\n            // If profile is valid, set up content\\n            // if (viewDetailsButton) viewDetailsButton.style.display = 'block'; // Button is removed\\n\\n            profileAvatarImg.src = profile.avatarUrl || '';\\n            profileAvatarImg.alt = profile.name ? `头像: ${profile.name}` : '头像';\\n            profileNameDisplay.textContent = profile.name || '未知姓名';\\n            profileTitleDisplay.textContent = profile.title || '暂无描述';\\n            \\n            currentProfileBio = profile.bio || '暂无简介';\\n            currentProfileStory = profile.story || ''; // Story is now empty\\n\\n            profileInfoDisplay.textContent = currentProfileBio; // Display bio initially\\n            isShowingStory = false; // Reset state\\n            // if (viewDetailsButton) viewDetailsButton.textContent = '查看详情'; // Button is removed\\n        }\\n\\n        initialShakeButton.addEventListener('click', () => {\\n            shakeScreen.style.display = 'none';\\n            profileScreen.style.display = 'flex';\\n            if (profiles.length > 0) {\\n                currentProfileIndex = Math.floor(Math.random() * profiles.length);\\n                displayProfile(profiles[currentProfileIndex]);\\n            } else {\\n                // Handle case with no profiles\\n                displayProfile(null); // Let displayProfile handle UI for no profile\\n            }\\n        });\\n\\n        chooseHerButton.addEventListener('click', () => {\\n            if (profiles.length === 0 || !profiles[currentProfileIndex]) return;\\n            const currentProfile = profiles[currentProfileIndex];\\n            \\n            // 构建要发送的文本\\n            // 将简介和剧情梗概中的换行符和多余空格处理掉，以避免在 requestText 中产生问题\\n            const cleanName = currentProfile.name.replace(/\\\\s+/g, ' ').trim();\\n            const cleanTitle = currentProfile.title.replace(/\\\\s+/g, ' ').trim();\\n            const cleanBio = currentProfile.bio.replace(/\\\\s+/g, ' ').trim();\\n            // const cleanStory = currentProfile.story.replace(/\\\\s+/g, ' ').trim(); // Story is now empty\\n            \\n            const profileDetails = `姓名: ${cleanName}, 描述: ${cleanTitle}, 简介: ${cleanBio}`; // Removed story from details\\n            const requestText = `<request:{{user}}查看了陌陌app上的（${profileDetails}）选择和她聊一聊>`;\\n\\n            try {\\n                if (typeof window.triggerSlash === 'function') {\\n                    console.log('Using window.triggerSlash to send:', requestText);\\n                    window.triggerSlash(`/send ${requestText} || /trigger`);\\n                } else if (window.parent && typeof window.parent.triggerSlash === 'function') {\\n                    console.log('Using window.parent.triggerSlash to send:', requestText);\\n                    window.parent.triggerSlash(`/send ${requestText} || /trigger`);\\n                } else {\\n                    console.warn('triggerSlash function not found. Attempting fallback...');\\n                    if (window.parent && window.parent.document) {\\n                        const textareaElement = window.parent.document.querySelector('#send_textarea');\\n                        const sendButton = window.parent.document.querySelector('#send_but');\\n                        const altTextarea = window.parent.document.querySelector('#prompt-input');\\n                        const altSendButton = window.parent.document.querySelector('#send_button') || window.parent.document.querySelector('button[type=\\\"submit\\\"]');\\n                        const targetTextarea = textareaElement || altTextarea;\\n                        const targetSendButton = sendButton || altSendButton;\\n                        if (targetTextarea && targetSendButton) {\\n                            console.log('Fallback: Found textarea and send button in parent. Sending:', requestText);\\n                            targetTextarea.value = requestText;\\n                            targetTextarea.dispatchEvent(new Event('input', { bubbles: true }));\\n                            targetSendButton.click();\\n                        } else {\\n                             console.error(`Fallback failed: Could not find textarea (tried #send_textarea, #prompt-input: ${!!targetTextarea}) or send button (tried #send_but, #send_button, button[type=submit]: ${!!targetSendButton}) in parent document.`);\\n                             if(targetTextarea) {\\n                                 console.log('Fallback: Found textarea, attempting to fill value only with:', requestText);\\n                                 targetTextarea.value = requestText;\\n                                 targetTextarea.dispatchEvent(new Event('input', { bubbles: true }));\\n                             }\\n                             // Fallback alert if sending fails\\n                             alert(`已选择: ${currentProfile.name}\\\\n简介: ${currentProfile.bio} (发送机制未找到)`);\\n                        }\\n                    } else {\\n                         console.error('Fallback failed: Cannot access parent window document.');\\n                         alert(`已选择: ${currentProfile.name}\\\\n简介: ${currentProfile.bio} (无法访问父窗口)`);\\n                    }\\n                }\\n            } catch (error) {\\n                console.error('Error sending choice via button:', error);\\n                alert(`已选择: ${currentProfile.name}\\\\n简介: ${currentProfile.bio} (发送时发生错误: ${error.message})`);\\n            }\\n        });\\n\\n        nextProfileButton.addEventListener('click', () => {\\n            if (profiles.length === 0) return;\\n            currentProfileIndex = Math.floor(Math.random() * profiles.length); // Always pick random\\n            displayProfile(profiles[currentProfileIndex]);\\n        });\\n\\n        // if (viewDetailsButton) { // Button is removed, so this block is no longer needed\\n        //     viewDetailsButton.addEventListener('click', () => {\\n        //         if (!profiles.length || currentProfileIndex < 0 || currentProfileIndex >= profiles.length || !profiles[currentProfileIndex]) { \\n        //             // No valid profile loaded\\n        //             return;\\n        //         }\\n\\n        //         if (isShowingStory) {\\n        //             profileInfoDisplay.textContent = currentProfileBio;\\n        //             viewDetailsButton.textContent = '查看详情';\\n        //             isShowingStory = false;\\n        //         } else {\\n        //             profileInfoDisplay.textContent = currentProfileStory;\\n        //             viewDetailsButton.textContent = '查看简介';\\n        //             isShowingStory = true;\\n        //         }\\n        //     });\\n        // }\\n\\n        // Initial display if needed (or handled by button click)\\n        // if (profiles.length > 0) {\\n        //     displayProfile(profiles[0]); // Display first profile initially\\n        // }\\n    </script>\\n</body>\\n</html>\\n\\n```\",\"trimStrings\":[],\"placement\":[1,2],\"disabled\":false,\"markdownOnly\":true,\"promptOnly\":false,\"runOnEdit\":true,\"substituteRegex\":0,\"minDepth\":null,\"maxDepth\":null},{\"id\":\"28dd40be-9b7b-4ffa-b283-f331e45fb18f\",\"scriptName\":\"女性状态栏\",\"findRegex\":\"==女性角色状态数据==\\\\s*姓名：\\\\[(.*?)\\\\]\\\\s*年龄职业：\\\\[(.*?)\\\\]\\\\s*当前服饰：\\\\[(.*?)\\\\]\\\\s*好感度：\\\\[(.*?)\\\\]\\\\s